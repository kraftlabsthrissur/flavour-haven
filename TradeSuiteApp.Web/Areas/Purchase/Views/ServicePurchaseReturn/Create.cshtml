<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_actions">
            <a  data-uk-tooltip="{pos:'bottom'}" title="Print"><i class="md-icon material-icons">&#xE8AD;</i></a>
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a >@Common.Action</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <h1>Purchase Return For Services</h1>
        <span class="uk-text-upper uk-text-small">Purchase Return</span>
    </div>
    <div id="page_content_inner">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="md-card-toolbar">
                    @ActionAuthorization.IsAuthorized("Save", Html.ActionLink(Common.SaveAsDraft, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " }))
                   @ActionAuthorization.IsAuthorized("Save", Html.ActionLink(Common.SaveAndNew, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " }))
                    
                    @Html.ActionLink(Common.Close, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " })
                </div>
                <h3 class="heading_a"></h3>

                <div class="uk-grid" data-uk-grid-margin>
                    <div class="uk-width-medium-1-1">
                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin>
                                <div class="uk-width-medium-1-10">
                                    <label>Return Number</label>
                                    <input type="text" class="md-input label-fixed" />
                                </div>
                                <div class="uk-width-medium-1-10">
                                    <div class="uk-input-group">
                                        <label for="uk_dp_start">Return Date</label>
                                        <input class="md-input label-fixed current-date date" type="text" id="uk_dp_start">
                                        <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar current-date"></i></span>
                                    </div>
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <div class="uk-input-group">
                                        <label>Supplier</label>
                                        <input type="text" class="md-input label-fixed label-fixed" />
                                        <span class="uk-input-group-addon"><a  data-uk-modal="{target:'#select_supplier'}"><i class="material-icons md-24"><b></b></i></a></span>
                                    </div>
                                </div>
                                <div class="uk-width-medium-1-10">
                                    <span><a class="md-btn"  data-uk-modal="{target:'#add_grn'}">@Purchase.AddGRN</a></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div class="tablescroll">
                    <table id="GRN-list" class="uk-table" cellspacing="0">
                        <thead>
                            <tr>
                                <th class="width-10">@Common.SerialNo</th>
                                <th class="width-10"></th>
                                <th class="width-250">Item</th>
                                <th class="width-40">Unit</th>
                                <th class="width-40">Approved Qty</th>
                                <th class="width-40">Return Qty</th>
                                <th class="width-40">Rate</th>
                                <th class="width-40">GST %</th>
                                <th class="width-60">GST Amount</th>
                                <th class="width-40">Total</th>
                                <th class="width-250">Remarks</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td class="uk-text-center">1</td>
                                <td class="checked uk-text-center" data-md-icheck> <input type="checkbox" /></td>
                                <td>Item 1</td>
                                <td>0</td>
                                <td class="uk-text-right">0</td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input label-fixed" /></td>
                            </tr>
                            <tr>
                                <td class="uk-text-center">2</td>
                                <td class="checked uk-text-center" data-md-icheck> <input type="checkbox" /></td>
                                <td>Item 2</td>
                                <td>0</td>
                                <td class="uk-text-right">0</td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input uk-text-right" /></td>
                                <td><input type="text" class="md-input label-fixed" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="uk-grid" data-uk-grid-margin>
                    <div class="uk-width-medium-1-1">
                        <div class="uk-grid" data-uk-grid-margin>
                            <div class="uk-width-medium-1-2">
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-4">
                                            <label>Freight</label>
                                            <input type="text" class="md-input label-fixed uk-text-right" />
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>Other Charges</label>
                                            <input type="text" class="md-input label-fixed uk-text-right" />
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>Packing Charges</label>
                                            <input type="text" class="md-input label-fixed uk-text-right" />
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>Net Amount</label>
                                            <input type="text" class="md-input label-fixed uk-text-right" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="uk-modal" id="add_grn" aria-hidden="true" style="display: none; overflow-y: scroll;">
                    <div class="uk-modal-dialog" style="">
                        <div class="uk-modal-header">
                            <h3 class="uk-modal-title">Select GRN <i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
                        </div>
                        <table id="purchase-requisition-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-30">@Common.SerialNo</th>
                                    <th class="width-30"></th>
                                    <th class="width-80">@Purchase.GRNNo</th>
                                    <th class="width-80">@Purchase.GRNDate</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="uk-text-center">1</td>
                                    <td class="uk-text-center checked" data-md-icheck><input type="checkbox" /></td>
                                    <td>GRN001</td>
                                    <td>02-Nov-2017</td>
                                </tr>
                                <tr>
                                    <td class="uk-text-center">2</td>
                                    <td class="uk-text-center checked" data-md-icheck><input type="checkbox" /></td>
                                    <td>GRN002</td>
                                    <td>02-Nov-2017</td>
                                </tr>
                                <tr>
                                    <td class="uk-text-center">3</td>
                                    <td class="uk-text-center checked" data-md-icheck><input type="checkbox" /></td>
                                    <td>GRN003</td>
                                    <td>02-Nov-2017</td>
                                </tr>
                            </tbody>
                        </table>
                        <div class="uk-modal-footer uk-text-right">
                            <button type="button" class="md-btn uk-modal-close">Close</button>
                            <button type="button" class="md-btn md-btn-primary uk-modal-close">Ok</button>
                        </div>
                    </div>
                </div>

                @Html.Partial("~/Areas/Masters/Views/Supplier/PartialList.cshtml")

                

                <div class="md-card-toolbar md-card-toolbar-bottom">
                    @Html.ActionLink(Common.SaveAsDraft, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " })
                    @Html.ActionLink(Common.SaveAndNew, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " })
                    @Html.ActionLink(Common.Close, "Create", "ServicePurchaseReturn", new { Area = "Purchase" }, new { @class = "md-btn " })
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/Supplier")