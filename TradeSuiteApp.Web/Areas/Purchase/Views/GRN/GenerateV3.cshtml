@model TradeSuiteApp.Web.Areas.Purchase.Models.GRNModel
@{
    ViewBag.Title = @Purchase.CreateNewGRN;
}
<div id="page_content">

    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Purchase.GRNForStockItems</h1>
            <span class="uk-text-upper uk-text-small">@Purchase.CreateNewGRN</span>
        </div>
        <div class="heading_actions">

            @Html.HiddenFor(m => m.IsBarCodeGenerator)
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn save-generate-grn'>" + Common.Save + "</a>")
            @Html.ActionLink(Common.Close, "IndexV3", "GRN", new { Area = "Purchase" }, new { @class = "md-btn " })

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <a href="~/Areas/Purchase/Views/GRN/GenerateV3.cshtml">~/Areas/Purchase/Views/GRN/GenerateV3.cshtml</a>
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    <div class="uk-form-row">
                        <div class="uk-grid" data-uk-grid-margin="">

                            <div class="uk-width-medium-8-10">
                                <div class="le ft-block">
                                    <div class="md-card-content">
                                        <ul class="uk-tab" data-uk-tab="{connect:'#tabs-purchase-GRM-create-content',animation:'slide-horizontal'}" id="tabs-purchase-GRM-create">
                                            <li class="uk-active" data-tab="main"><a>Main Fields</a></li>
                                            <li class="" data-tab="optional"><a>Optional Fields</a></li>
                                        </ul>
                                        <ul id="tabs-purchase-GRM-create-content" class="uk-switcher uk-margin">
                                            <li>
                                                <div class="uk-form-row">
                                                    <div class="uk-grid" data-uk-grid-margin>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.GRNNo</label>
                                                            @Html.TextBoxFor(m => m.GRNNo, new { @class = "md-input", @disabled = "disabled" })
                                                        </div>
                                                        @Html.Hidden("Date", @DateTime.Now.ToString("dd-MM-yyyy"))
                                                        <div class="uk-width-medium-1-8">
                                                            <label for="uk_dp_start">@Purchase.GRNDate</label>
                                                            <input type="text" class="md-input label-fixed " id="txtDate" value="@DateTime.Now.ToString("dd-MM-yyyy")" disabled />

                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.GrossAmount</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="GrossAmt" value="0" Disabled="disabled" />

                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.DiscountPercentage</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="DiscountPercent" value="0" />
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.Discount</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="DiscountAmt" value="0" />

                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.VATPercentage</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="VATPercentage" value="0" />
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.VATAmount</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="VATAmount" value="0" />
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.SuppDocAmount</label>
                                                            @Html.TextBoxFor(model => model.SuppDocAmount, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="uk-form-row">
                                                    <div class="uk-grid" data-uk-grid-margin>

                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.SuppShipAmount</label>
                                                            @Html.TextBoxFor(model => model.SuppShipAmount, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.PackingForwarding</label>
                                                            @Html.TextBoxFor(model => model.PackingForwarding, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.SupplierOtherCharges</label>
                                                            @Html.TextBoxFor(model => model.SuppOtherCharges, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.SuppFreight</label>
                                                            @Html.TextBoxFor(model => model.SuppFreight, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.LocalCustomsDuty</label>
                                                            @Html.TextBoxFor(model => model.LocalCustomsDuty, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.LocalFreight</label>
                                                            @Html.TextBoxFor(model => model.LocalFreight, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>

                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.LocalMiscCharge</label>
                                                            @Html.TextBoxFor(model => model.LocalMiscCharge, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.LocalOtherCharges</label>
                                                            @Html.TextBoxFor(model => model.LocalOtherCharges, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.ChallanNo</label>
                                                            <input type="text" class="md-input label-fixed" id="invoice-number" />
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <div class="uk-input-group">
                                                                <label for="uk_dp_start">@Purchase.ChallanDate</label>
                                                                <input class="md-input label-fixed past-date date" type="text" id="invoice-date">
                                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </li>

                                            <li>
                                                <div class="uk-form-row">
                                                    <div class="uk-grid" data-uk-grid-margin="">
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.TaxableAmount</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="TaxableAmount" value="0" disabled="disabled" />

                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.Location</label>
                                                            @Html.DropDownListFor(Model => Model.WarehouseID, new SelectList(Model.WarehoueList, "ID", "Name"), "Select", new { @class = "md-input label-fixed", @id = "StoreID" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Masters.CurrencyName</label>
                                                            @Html.TextBoxFor(model => model.CurrencyName, new { @class = "md-input label-fixed", @tabindex = "-1", @disabled = "disabled" })
                                                        </div>
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Masters.ExchangeRate</label>
                                                            @Html.TextBoxFor(model => model.CurrencyExchangeRate, new { @class = "md-input label-fixed " + Model.normalclass, @tabindex = "-1", @disabled = "disabled" })
                                                        </div>
                                                
                                                        <div class="uk-width-medium-1-8">
                                                            <label>@Purchase.RoundOff</label>
                                                            <input type="text" class="md-input label-fixed mask-currency" id="RoundOff" value="0" Disabled="disabled" />
                                                        </div>
                         
                                                        <div class="uk-width-medium-2-8">
                                                            <label>@Masters.Remarks</label>
                                                            @Html.TextBoxFor(model => model.Remarks, new { @class = "md-input label-fixed", @tabindex = "-1" })
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="uk-form-row">
                                                    <div class="uk-grid" data-uk-grid-margin="">
                                                    
                                                    </div>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>





                                </div>
                            </div>
                            <div class="uk-width-medium-2-10">
                                <div class="right-block">

                                    <div class="uk-form-row">
                                        <div class="uk-grid" data-uk-grid-margin="">

                                            <div class="uk-width-medium-1-1">
                                                <label>@Purchase.NetAmount</label>
                                                <input type="text" class="md-input label-fixed mask-qty x-large" id="NetAmt" value="0" disabled />
                                            </div>
                                            <div class="uk-width-medium-1-1">
                                                <label>@Purchase.LocalLandingCost</label>
                                                <input type="text" class="md-input label-fixed mask-qty x-large" id="LocalLandinngCost" value="0" disabled />
                                            </div>
                                            <div class="uk-width-medium-1-1">
                                                <label>@Masters.CurrencyCode</label>
                                                @Html.TextBoxFor(model => model.CurrencyCode, new { @class = "md-input label-fixed", @tabindex = "-1", @Disabled = "disabled" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="section-group">
                    <div class="uk-grid" data-uk-grid-margin="">

                        <div class="uk-width-medium-2-8">
                            <div class="uk-autocomplete " id="supplier-autocomplete">
                                <div class="uk-input-group">
                                    <label>@Purchase.Supplier</label>
                                    @Html.TextBox("SupplierName", "", new { @class = "md-input label-fixed" })
                                    <span class="uk-input-group-addon" data-uk-modal="{target:'#select-supplier',center:false}">
                                        <i class="uk-input-group-icon uk-icon-search"></i>
                                    </span>
                                    <script type="text/autocomplete">
                                           <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                               {{~items}}
                                               <li data-value="{{ $item.Name }}"
                                                   data-id="{{$item.ID}}"
                                                   data-location="{{$item.Location}}"
                                                   data-state-id="{{$item.StateID}}"
                                                   data-is-gst-registered="{{$item.IsGSTRegistered}}">
                                                   <a>
                                                       {{ $item.Name }} ({{{ $item.Code }}})
                                                       <div>{{{ $item.Location }}}</div>
                                                   </a>
                                               </li>
                                               {{/items}}
                                           </ul>
                                    </script>
                                </div>
                            </div>
                            @Html.HiddenFor(m => m.SupplierID, new { @class = "md-input label-fixed" })
                            @Html.Hidden("Code")
                            @*@Html.Hidden("Date")*@
                            @Html.HiddenFor(m => m.ShippingStateID)
                            @Html.HiddenFor(m => m.StateID)
                            @Html.HiddenFor(m => m.IsGSTRegistered)
                            @Html.HiddenFor(m => m.PurchaseOrderDate)
                            @Html.HiddenFor(m => m.IsGST)
                            @Html.HiddenFor(m => m.IsVat)
                            @Html.HiddenFor(m => m.TaxTypeID)
                            @Html.HiddenFor(m => m.CurrencyID)
                            @Html.HiddenFor(m => m.DecimalPlaces)
                            @Html.HiddenFor(m => m.normalclass)
                            @Html.Hidden("ItemIDHidden")


                        </div>

                        <div class="uk-width-medium-1-8 grn">
                            <div class="uk-input-group">
                                <label>@Purchase.SelectPurchaseOrder</label>
                                <input type="text" class="md-input label-fixed" id="txt-purchase-order" />
                                <span class="uk-input-group-addon" data-uk-modal="{target:'#select_po',center:false }">
                                    <i class="uk-input-group-icon uk-icon-search"></i>
                                </span>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="section-group">
                    <div class="uk-form-row">
                        <table id="grn-items-list" class="uk-table" cellspacing="0">
                            <thead>
                                <tr>
                                    <th class="width-10">@Common.SerialNo</th>
                                    <th class="width-10"></th>
                                    <th class="width-60">@Purchase.ItemCode</th>
                                    <th class="width-100">@Purchase.ItemName</th>
                                    <th class="width-100 uk-hidden">@Purchase.PartsNumber</th>
                                    <th class="width-60">@Purchase.Remarks</th>
                                    <th class="width-60">@Masters.Model</th>
                                    <th class="width-60">@Purchase.Bin</th>
                                    <th class="width-60">@Purchase.PONo</th>
                                    <th class="width-60">@Purchase.Unit</th>
                                    @*<th class="width-80">@Purchase.Batch</th>*@
                                    @*<th class="width-30">@Purchase.POQty</th>*@
                                    <th class="width-50">@Purchase.PurchaseRate</th>
                                    <th class="width-60">@Purchase.GrossAmount</th>
                                    <th class="width-40">@Purchase.ReceivedQty</th>
                                    <th class="width-40">@Purchase.OfferQty</th>
                                    <th class="width-30 ">@Purchase.DiscountPer</th>
                                    <th class="width-50 ">@Purchase.Discount</th>
                                    @*<th class="width-60">@Purchase.GSTper</th>
            <th class="width-60">@Purchase.SGSTAmt</th>
            <th class="width-60">@Purchase.SGSTAmt</th>
            <th class="width-60">@Purchase.IGSTAmt</th>
        <th class="width-80">@Purchase.TaxableAmount</th>*@
                                    <th class="width-30">@Purchase.VATPercentage</th>
                                    <th class="width-50">@Purchase.VATAmount</th>
                                    <th class="width-60">@Purchase.TaxableAmount</th>
                                    <th class="width-60">@Purchase.NetAmount</th>
                                </tr>
                            </thead>
                            <tbody class="tbody"></tbody>
                        </table>
                        <input type="hidden" id="item-count" value="0" />
                        <input type="hidden" id="invoice-count" value="0" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="uk-modal " id="select_po" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog " style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Select Purchase Order <i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="purchase-order-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-10">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-40">@Purchase.TransNo</th>
                    <th class="width-250">@Purchase.PODate</th>
                    <th class="width-250">@Purchase.Supplier</th>
                    <th class="width-250">@Purchase.RequestedBy</th>
                    <th class="width-40">@Purchase.NetAmount</th>


                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th class="width-10">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-40">@Purchase.TransNo</th>
                    <th class="width-250">@Purchase.PODate</th>
                    <th class="width-250">@Purchase.Supplier</th>
                    <th class="width-250">@Purchase.RequestedBy</th>
                    <th class="width-40">@Purchase.NetAmount</th>


                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKPurchaseOrders">Ok</button>

        </div>
    </div>
</div>
@Html.Partial("~/Areas/Masters/Views/Supplier/PartialList.cshtml")
@Html.Partial("~/Areas/Purchase/Views/GRN/_batchWithStock.cshtml")
@Html.Partial("~/Areas/Purchase/Views/GRN/_createbatchV3.cshtml", Model)
@Html.Partial("~/Areas/Purchase/Views/GRN/QRCodeGenerate.cshtml")
@Html.Partial("~/Areas/Purchase/Views/GRN/BarCodeGenerate.cshtml")
@Html.Partial("~/Areas/Purchase/Views/GRN/_getItemDetails.cshtml")
@Scripts.Render(System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Js/Page/Item"))
@Scripts.Render(System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Js/Page/Supplier"))
@Scripts.Render(System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Js/Page/GRNV3"))
<script type="text/javascript">
    GRN.init();
</script>