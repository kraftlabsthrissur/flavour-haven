@model TradeSuiteApp.Web.Areas.Purchase.Models.SRNViewModel
@{
    ViewBag.Title = Purchase.CreateNewSRN;

}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Purchase.SRN</h1>
            <span class="uk-text-upper uk-text-small">@Purchase.CreateNewSRN</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveSRN'>" + Common.Save + "</a>")
            @ActionAuthorization.IsAuthorized("SaveAsDraft", "<a class='md-btn btnSaveDraftSRN'>" + Common.SaveAsDraft + "</a>")
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveAndNewSRN'>" + Common.SaveAndNew + "</a>")
            @Html.ActionLink(Common.Close, "Index", "ServiceSRN", new { Area = "Purchase" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Purchase/Views/ServiceSRN/_form.cshtml", Model); }


            
            </div>
        </div>
    </div>
</div>


@Html.Partial("~/Areas/Masters/Views/Supplier/PartialList.cshtml")
@Scripts.Render("~/Js/Page/Supplier")
@Scripts.Render("~/Js/Page/ServiceSRN")
<script type="text/javascript">
    SRN.init();
</script>