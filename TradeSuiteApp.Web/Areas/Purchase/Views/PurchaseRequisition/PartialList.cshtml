<div class="uk-modal" id="select-purchase-requisition" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Purchase.PurchaseRequisition<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="purchase-requisition-list" class="uk-table" cellspacing="0">
            <thead>
                <tr>
                    <th class="width-40">@Common.SerialNo</th>
                    <th class="width-20"></th>
                    <th class="width-80">@Purchase.TransNo</th>
                    <th class="width-80">@Purchase.TransDate/th>
                    <th class="width-80">@Purchase.SupplierName</th>
                    <th class="width-100">@Purchase.CustomerPhoneNo</th>
                    <th class="width-200">@Purchase.CustomerAddres</th>
                    <th class="width-100">@Purchase.Remarks</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th></th>
                    <th></th>
                    <th>@Purchase.TransNo</th>
                    <th>@Purchase.TransDate</th>
                    <th>@Purchase.SupplierName</th>
                    <th>@Purchase.CustomerPhoneNo</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOkPurchaseRequisition">Ok</button>
        </div>
    </div>
</div>