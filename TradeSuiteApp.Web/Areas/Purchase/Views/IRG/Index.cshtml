@model IEnumerable<TradeSuiteApp.Web.Areas.Purchase.Models.PurchaseReturnModel>
@{
    ViewBag.Title = Purchase.IRG;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Purchase.IRGStockItems</h1>
            <span class="uk-text-upper uk-text-small">@Purchase.IRG</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "IRG", new { Area = "Purchase" }, new { @class = "md-btn" }))
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-IRG-content',animation:'slide-horizontal'}" id="tabs-IRG">
                    <li class="uk-active" data-tab="draft"><a>@Purchase.Draft</a></li>
                    <li class="" data-tab="saved-IRG-note"><a>@Purchase.SavedIRG</a></li>
                </ul>
                <ul id="tabs-IRG-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        Trans No
                                    </th>
                                    <th class="width-80">
                                        @Stock.TransDate
                                    </th>
                                    <th>
                                        Supplier
                                    </th>

                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        Trans No
                                    </th>
                                    <th class="width-80">
                                        @Stock.TransDate
                                    </th>
                                    <th>
                                        Supplier
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="saved-IRG-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        Trans No
                                    </th>
                                    <th class="width-80">
                                        @Stock.TransDate
                                    </th>
                                    <th>
                                        Supplier
                                    </th>

                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        Trans No
                                    </th>
                                    <th class="width-80">
                                        @Stock.TransDate
                                    </th>
                                    <th>
                                        Supplier
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/IRG")
<script type="text/javascript">
    irg.list();
</script>