


<div class="uk-modal" id="add-invoice" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Select Purchase Invoice<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <br />

        <table id="invoice-popup-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-20"></th>
                    <th class="width-40">@Purchase.TransNo</th>
                    <th class="width-40">@Sales.InvoiceNo</th>
                    <th class="width-40">@Sales.InvoiceDate</th>
                    <th class="width-40">@Sales.GrossAmt</th>

                </tr>
            </thead>
            <thead class="search">
                <tr>

                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-20"></th>
                    <th class="width-40">@Purchase.TransNo</th>
                    <th class="width-40">@Sales.InvoiceNo</th>
                    <th class="width-40">@Sales.InvoiceDate</th>
                    <th class="width-40">@Sales.GrossAmt</th>

                </tr>

            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn md-btn-flat uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-flat uk-modal-close" id="btnOkInvoiceList">Ok</button>
        </div>
    </div>
</div>



