                                                            <li data-value="{{$item.Code }}"
                                                                data-id="{{$item.ID}}">
                                                                <a>
                                                                    {{$item.Code }}
                                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                                </a>
                                                            </li>
                                                            {{/items}}
                                                        </ul>
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    @Html.HiddenFor(m => m.GRNNoToID)
                                </div>
                                <!--GRN Date-->
