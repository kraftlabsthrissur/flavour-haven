                                            <div class="uk-input-group">
                                                <label class="prdate">@Reports.GRNFromDate</label>
                                                @Html.TextBoxFor(model => model.GRNDateFrom, new { @class = "md-input label-fixed  past-date date", @id = "GRNDateFrom" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-input-group">
                                                <label class="prdateto">@Reports.GRNToDate</label>
                                                @Html.TextBoxFor(model => model.GRNDateTo, new { @class = "md-input label-fixed  past-date date", @id = "GRNDateTo" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Request Date-->
                                <div class="uk-form-row summary">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-input-group">
                                                <label class="prdate">@Reports.RequestDateFrom</label>
                                                @Html.TextBoxFor(model => model.RequestDateFrom, new { @class = "md-input label-fixed  past-date date", @id = "RequestDateFrom" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-input-group">
                                                <label class="prdateto">@Reports.RequestDateTo</label>
                                                @Html.TextBoxFor(model => model.RequestDateTo, new { @class = "md-input label-fixed  past-date date", @id = "RequestDateTo" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!--Delivered Date-->
                                <div class="uk-form-row summary">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-input-group">
                                                <label class="DeliveredDateFrom">@Reports.DeliveredDateFrom</label>
                                                @Html.TextBoxFor(model => model.DeliveredDateFrom, new { @class = "md-input label-fixed  past-date date", @id = "DeliveredDateFrom" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-input-group">
                                                <label class="DeliveredDateTo">@Reports.DeliveredDateTo</label>
                                                @Html.TextBoxFor(model => model.DeliveredDateTo, new { @class = "md-input label-fixed  past-date date", @id = "DeliveredDateTo" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Location-->
                                <div class="uk-form-row ">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2  ">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start" class="LocationFrom">@Reports.LocationFrom</label>
                                                <select id="LocationFromID" name="LocationFromID" class="md-input label-fixed">
                                                    <option value="0">Select</option>
                                                    @foreach (var location in Model.LocationHeadList)
                                        {
                                        <option @(Model.LocationFromID == location.ID ? "selected='selected'" : "") value="@location.ID" data-type="@location.LocationType" data-code="@location.Code" data-headid="@location.LocationHeadID">@location.Name</option>
                                        }
                                                </select>
                                                @*@Html.DropDownListFor(Model => Model.LocationFromID, Model.LocationList, "Select", new { @class = "md-input label-fixed", @id = "LocationFromID" })*@
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2 location">
                                            <label for="uk_dp_start" class="LocationTo">@Reports.LocationTo</label>
                                            <select id="LocationToID" name="LocationToID" class="md-input label-fixed">
                                                <option value="0">Select</option>
                                                @foreach (var location in Model.LocationHeadList)
                                    {
                                    <option @(Model.LocationToID == location.ID ? "selected='selected'" : "") value="@location.ID" data-type="@location.LocationType" data-code="@location.Code" data-headid="@location.LocationHeadID">@location.Name</option>
                                    }
                                            </select>

                                            @*@Html.DropDownListFor(Model => Model.LocationToID, Model.LocationList, "Select", new { @class = "md-input label-fixed", @id = "LocationToID" })*@
                                        </div>
                                    </div>
                                </div>
                                <!--Premises -->
                                <div class="uk-form-row premises">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2  ">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start" class="PremisesFrom">@Reports.PremisesFrom</label>
                                                @Html.DropDownListFor(Model => Model.PremisesFromID, Model.PremiseList, "Select", new { @class = "md-input label-fixed", @id = "PremisesFromID" })
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2 ">
                                            <label for="uk_dp_start" class="PremisesTo">@Reports.PremisesTo</label>
                                            @Html.DropDownListFor(Model => Model.PremisesToID, Model.PremiseList, "Select", new { @class = "md-input label-fixed", @id = "PremisesToID" })
                                        </div>
                                    </div>
                                </div>
                                <!--Item Category -->
                                @*<div class="uk-form-row">
                                        <div class="uk-grid" data-uk-grid-margin>
                                            <div class="uk-width-medium-1-3">
                                                <label>@Reports.ItemCategoryFrom</label>
                                                @Html.DropDownListFor(Model => Model.FromItemCategoryRange, Model.FromItemCategoryRangeList, "Select", new { @class = "md-input label-fixed", @id = "ItemCategoryFromRange" })
                                            </div>
                                            <div class="uk-width-medium-1-3">
                                                <label>@Reports.ItemCategoryTo</label>
                                                @Html.DropDownListFor(Model => Model.ToItemCategoryRange, Model.ToItemCategoryRangeList, "Select", new { @class = "md-input label-fixed", @id = "ItemCategoryToRange" })
                                            </div>
                                        </div>
                                    </div>*@
                                <!--Item Category -->
                                <div class="uk-form-row ">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-1  ">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start" class="RequestNo">@Reports.Itemcategory</label>
                                                @Html.DropDownListFor(Model => Model.ItemCategoryID, Model.ItemCategoryList, "Select", new { @class = "md-input label-fixed", @id = "ItemCategoryID" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Item Code -->
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-2">
                                            <div class="uk-autocomplete " id="itemcode-from-autocomplete">
                                                <label for="uk_dp_start">@Reports.ItemCodeFrom</label>
                                                @Html.TextBox("ItemCodeFrom", "", new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
