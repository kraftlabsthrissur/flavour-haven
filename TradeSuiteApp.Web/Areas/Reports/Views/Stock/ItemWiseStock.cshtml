@model TradeSuiteApp.Web.Areas.Reports.Models.ItemMovementModel
@{
    ViewBag.Title = "ItemWiseStock";
    ViewBag.ReportTitle = "Item Wise Stock";
}
@Scripts.Render("~/Js/Page/Stock")
<div id="page_content_inner" class="purchase-report">
    <div class="md-card uk-margin-medium-bottom">
        <div class="md-card-content">
            <div class="section-group">
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin="">
                        <div class="uk-width-medium-2-10 ">
                            <form action="/Reports/Stock/ItemWiseStock" id="report-filter-form" method="POST">
                                @Html.HiddenFor(m => m.LocationID)
                                @Html.HiddenFor(m => m.UserID)
                                <!--Report Type-->
                                <div class="uk-form-row">
                                    <div class="panel">
                                        <label>Report Type</label>
                                        <div class="uk-form-row">
                                            <div class="uk-width-medium-1-1">
                                                <input type="radio" id="ItemMovement" name="ReportType" class="reportType" data-md-icheck checked="" value="Item Movement" /> @Html.Label("Item Movement")
                                            </div>
                                        </div>
                                        <br />
                                        <div class="uk-form-row">
                                            <div class="uk-width-medium-1-1">
                                                <input type="radio" id="ItemStatus" name="ReportType" class="reportType" data-md-icheck value="Item Status" /> @Html.Label("Item Status")
                                            </div>
                                        </div>
                                        <br />
                                        <div class="uk-form-row">
                                            <div class="uk-width-medium-1-1">
                                                <input type="radio" name="ReportType" class="reportType" data-md-icheck value="ItemwiseInput" /> @Html.Label("Itemwise Production Input Report")
                                            </div>
                                        </div>
                                        <br />
                                        <div class="uk-form-row">
                                            <div class="uk-width-medium-1-1">
                                                <input type="radio" name="ReportType" class="reportType" data-md-icheck value="ItemwiseOutput" /> @Html.Label("Itemwise Production Output Report")
                                            </div>
                                        </div>
                                        <br />
                                        <div class="uk-form-row">
                                            <div class="uk-width-medium-1-1">
                                                <input type="radio" name="ReportType" class="reportType" data-md-icheck value="ItemwisePurchase" /> @Html.Label("Itemwise purchase Report")
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="uk-form-row ratevalue">
                                    <div class="panel">
                                        <label>Rate & Value</label>
                                        <div class="uk-grid" data-uk-grid-margin>
                                            <div class="uk-width-medium-1-2">
                                                <input type="radio" name="RateValue" class="rateValue" data-md-icheck checked="" value="Yes" /> @Html.Label("Yes")
                                            </div>
                                            <div class="uk-width-medium-1-2">
                                                <input type="radio" name="RateValue" class="rateValue" data-md-icheck value="No" /> @Html.Label("No")
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Request Number-->
                                <div class="uk-form-row request-no uk-hidden">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2  ">
                                            @*<div class="uk-input-group">*@
                                            <div class="uk-autocomplete " id="stock-requestno-autocomplete">
                                                <label for="uk_dp_start" class="RequestNo">@Reports.RequestNoFrom</label>
                                                @Html.TextBoxFor(model => model.RequestNoFrom, new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
                                                    <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                        {{~items}}
                                                        <li data-value="{{ $item.Code }}"
                                                            data-id="{{$item.ID}}">
                                                            <a>
                                                                {{ $item.Code }}
                                                                @*<div>{{{ $item.ID }}}</div>*@
                                                            </a>
                                                        </li>
                                                        {{/items}}
                                                    </ul>
                                                </script>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2 ">
                                            <div class="uk-input-group">
                                                <div class="uk-autocomplete " id="stock-requestnoTo-autocomplete">
                                                    <label for="uk_dp_start" class="RequestNoTo">@Reports.RequestNoTo</label>
                                                    @Html.TextBoxFor(model => model.RequestNoTo, new { @class = "md-input label-fixed" })
                                                    <script type="text/autocomplete">
                                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                            {{~items}}
                                                            <li data-value="{{ $item.Code }}"
                                                                data-id="{{$item.ID}}">
                                                                <a>
                                                                    {{ $item.Code }}
                                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                                </a>
                                                            </li>
                                                            {{/items}}
                                                        </ul>
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    @Html.HiddenFor(m => m.RequestNoFromID)
                                    @Html.HiddenFor(m => m.RequestNoToID)
                                </div>


                                <!--Purchase Order Number-->

                                <div class="uk-form-row itemwise-purchase uk-hidden">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2  ">
                                            <div class="uk-input-group">
                                                <div class="uk-autocomplete " id="purchase-order-no-autocomplete">
                                                    <label for="uk_dp_start" class="RequestNo">@Reports.PurchaseOrderFrom</label>
                                                    @Html.TextBoxFor(model => model.purchaseOrderNoFrom, new { @class = "md-input label-fixed" })
                                                    <script type="text/autocomplete">
                                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                            {{~items}}
                                                            <li data-value="{{ $item.Code }}"
                                                                data-id="{{$item.ID}}">
                                                                <a>
                                                                    {{ $item.Code }}
                                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                                </a>
                                                            </li>
                                                            {{/items}}
                                                        </ul>
                                                    </script>
                                                </div>
                                            </div>
                                            @Html.HiddenFor(m => m.purchaseOrderNOFromID)
                                        </div>
                                        <div class="uk-width-medium-1-2 ">
                                            <div class="uk-input-group">
                                                <div class="uk-autocomplete " id="purchase-order-no-to-autocomplete">
                                                    <label for="uk_dp_start" class="RequestNoTo">@Reports.PurcahseOrderTo</label>
                                                    @Html.TextBoxFor(model => model.PurchaseOrderNoTo, new { @class = "md-input label-fixed" })
                                                    <script type="text/autocomplete">
                                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                            {{~items}}
                                                            <li data-value="{{$item.Code }}"
                                                                data-id="{{$item.ID}}">
                                                                <a>
                                                                    {{$item.Code }}
                                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                                </a>
                                                            </li>
                                                            {{/items}}
                                                        </ul>
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    @Html.HiddenFor(m => m.PurchaseOrderNoToID)
                                </div>
                                <!--GRN NO -->
                                <div class="uk-form-row itemwise-purchase uk-hidden">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-2  ">
                                            <div class="uk-input-group">
                                                <div class="uk-autocomplete " id="GRNNOFrom-autocomplete">
                                                    <label for="uk_dp_start" class="RequestNo">@Reports.GRNNOFrom</label>
                                                    @Html.TextBoxFor(model => model.GRNNoFrom, new { @class = "md-input label-fixed" })
                                                    <script type="text/autocomplete">
                                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                            {{~items}}
                                                            <li data-value="{{$item.Code }}"
                                                                data-id="{{$item.ID}}">
                                                                <a>
                                                                    {{$item.Code }}
                                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                                </a>
                                                            </li>
                                                            {{/items}}
                                                        </ul>
                                                    </script>
                                                </div>
                                            </div>
                                            @Html.HiddenFor(m => m.GRNNoFromID)
                                        </div>
                                        <div class="uk-width-medium-1-2 ">
                                            <div class="uk-input-group">
                                                <div class="uk-autocomplete " id="GRNNoTo-autocomplete">
                                                    <label for="uk_dp_start" class="RequestNoTo">@Reports.GRNNOTo</label>
                                                    @Html.TextBoxFor(model => model.GRNNoTo, new { @class = "md-input label-fixed" })
