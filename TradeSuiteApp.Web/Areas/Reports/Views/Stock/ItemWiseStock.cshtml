                                            <div class="uk-autocomplete ">
                                                <label for="uk_dp_start">@Reports.StatusFrom</label>
                                                @Html.DropDownListFor(Model => Model.StatusFrom, Model.StatusList, new { @class = "md-input label-fixed", @id = "StatusFrom" })
