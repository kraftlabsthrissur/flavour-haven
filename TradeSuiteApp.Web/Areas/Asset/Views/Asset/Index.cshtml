

@model  TradeSuiteApp.Web.Areas.Asset.Models.AssetFilterModel
@{
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Asset.Assets</h1>
            <span class="uk-text-upper uk-text-small">@Asset.AssetList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Filter", "<a class='md-btn btnFilter'>" + Asset.Filter + "</a>")
            
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    <div class="uk-form-row">
                        <div class="uk-grid filter uk-hidden" data-uk-grid-margin="">
                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-autocomplete " id="transno-from-autocomplete">
                                    <label for="uk_dp_start">@Asset.TransNoFrom</label>
                                    @Html.TextBox("TransNoFrom", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Code }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Code }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
                                            {{/items}}
                                        </ul>
                                    </script>
                                </div>
                                @Html.HiddenFor(m => m.TransNoFromID)
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-autocomplete " id="transno-to-autocomplete">
                                    <label for="uk_dp_start">@Asset.TransNoTo</label>
                                    @Html.TextBox("TransNoTo", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Code }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Code }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
                                            {{/items}}
                                        </ul>
                                    </script>
                                </div>
                                @Html.HiddenFor(m => m.TransNoToID)
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-input-group">
                                    <label for="uk_dp_start">@Asset.TransDateFrom</label>
                                    @Html.TextBoxFor(model => model.TransDateFromStr, new { @class = "md-input label-fixed  past-date date" })
                                    <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                </div>
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-input-group">
                                    <label for="uk_dp_start">@Asset.TransDateTo</label>
                                    @Html.TextBoxFor(model => model.TransDateToStr, new { @class = "md-input label-fixed  past-date date" })
                                    <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                </div>
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-autocomplete " id="receiptno-from-autocomplete">
                                    <label for="uk_dp_start">@Asset.ReceiptNoFrom</label>
                                    @Html.TextBox("ReceiptNoFrom", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Code }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Code }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
