                                                    </th>
                                                    <th>
                                                        @Asset.TransNo
                                                    </th>
                                                    <th>
                                                        @Asset.AssetCode
                                                    </th>
                                                    <th>
                                                        @Asset.AssetName
                                                    </th>
                                                    <th>
                                                        @Asset.ItemName
                                                    </th>
