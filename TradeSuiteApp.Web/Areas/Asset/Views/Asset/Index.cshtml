                                        <table id="revenue-asset-items-list" class="uk-table" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        @Common.SerialNo
                                                    </th>
                                                    <th>
                                                        @Asset.TransNo
                                                    </th>
                                                    <th>
                                                        @Asset.AssetCode
                                                    </th>
