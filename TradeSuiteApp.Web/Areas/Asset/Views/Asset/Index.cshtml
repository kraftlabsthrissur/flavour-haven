                                <div class="uk-autocomplete " id="receiptno-to-autocomplete">
                                    <label for="uk_dp_start">@Asset.ReceiptNoTo</label>
                                    @Html.TextBox("ReceiptNoTo", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Code }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Code }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
                                            {{/items}}
                                        </ul>
                                    </script>
                                </div>
                                @Html.HiddenFor(m => m.ReceiptNoToID)
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.AssetNameFrom</label>
                                @Html.DropDownListFor(Model => Model.AssetNameFrom, Model.AssetNameRangeList, "Select", new { @class = "md-input label-fixed", @id = "FromAssetRange" })
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.AssetNameTo</label>
                                @Html.DropDownListFor(Model => Model.AssetNameTo, Model.AssetNameRangeList, "Select", new { @class = "md-input label-fixed", @id = "ToAssetRange" })
                            </div>
                            <div class="uk-width-medium-2-10 ">
                                <div class="uk-autocomplete " id="asset-name-autocomplete">
                                    <label for="uk_dp_start">@Asset.AssetName</label>
                                    @Html.TextBox("AssetName", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Code }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Code }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
                                            {{/items}}
                                        </ul>
                                    </script>
                                </div>
                                @Html.HiddenFor(m => m.AssetNameID)
                            </div>

                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.AccountCategoryFrom</label>
                                @Html.DropDownListFor(Model => Model.AccountCategoryFrom, Model.AccountCategoryRangeList, "Select", new { @class = "md-input label-fixed", @id = "FromAccountCategoryRange" })
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.AccountCategoryTo</label>
                                @Html.DropDownListFor(Model => Model.AccountCategoryTo, Model.AccountCategoryRangeList, "Select", new { @class = "md-input label-fixed", @id = "ToAccountCategoryRange" })
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.AccountCategory</label>
                                @Html.DropDownListFor(Model => Model.AccountCategoryID, Model.AccountCategoryList, "Select", new { @class = "md-input label-fixed", @id = "AccountCategory" })
                            </div>

                            <div class="uk-width-medium-1-10 ">
                                <div class="uk-input-group">
                                    <label for="uk_dp_start">@Asset.SupplierNameFrom</label>
                                    @Html.DropDownListFor(Model => Model.SupplierNameFrom, Model.SupplierNameRangeList, "Select", new { @class = "md-input label-fixed", @id = "FromSupplierNameRange" })
                                </div>
                            </div>
                            <div class="uk-width-medium-1-10 ">
                                <label for="uk_dp_start">@Asset.SupplierNameTo</label>
                                @Html.DropDownListFor(Model => Model.SupplierNameTo, Model.SupplierNameRangeList, "Select", new { @class = "md-input label-fixed", @id = "ToSupplierNameRange" })
                            </div>
                            <div class="uk-width-medium-2-10 ">
                                <div class="uk-autocomplete " id="SupplierName-autocomplete">
                                    <label for="uk_dp_start">@Asset.SupplierName</label>
                                    @Html.TextBox("SupplierName", "", new { @class = "md-input label-fixed" })
                                    <script type="text/autocomplete">
                                        <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                            {{~items}}
                                            <li data-value="{{ $item.Name }}"
                                                data-id="{{$item.ID}}">
                                                <a>
                                                    {{ $item.Name }}
                                                    @*<div>{{{ $item.ID }}}</div>*@
                                                </a>
                                            </li>
                                            {{/items}}
                                        </ul>
                                    </script>
                                </div>
                                @Html.HiddenFor(m => m.SupplierID)
                            </div>
                            <div class="uk-width-medium-1-10">
                                <div class="md-input-wrapper md-input-filled">
                                    <label>&nbsp;</label>
                                    <button class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="btnSubmit">@Asset.Submit</button>
                                </div>
                            </div>
                            <div class="uk-width-medium-1-10">
                                <div class="md-input-wrapper md-input-filled">
                                    <label>&nbsp;</label>
                                    <button class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="btnReset">@Asset.Reset</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="section-group">
                    <div class="uk-form-row">
                        <div class="uk-grid">
                            <div class="uk-width-1-1">
                                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-asset-content',animation:'slide-horizontal'}" id="tabs-qc">
                                    <li class="uk-active"><a >@Asset.Pending</a></li>
                                    <li><a >@Asset.Capitalise</a></li>
                                    <li><a >@Asset.Revenue</a></li>
                                </ul>
                                <ul id="tabs-asset-content" class="uk-switcher uk-margin">
