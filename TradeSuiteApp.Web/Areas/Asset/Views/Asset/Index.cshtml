                                        </table>
                                    </li>
                                    <li>
                                        <table id="capital-asset-items-list" class="uk-table" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        @Common.SerialNo
                                                    </th>
                                                    <th>
                                                        @Asset.TransNo
                                                    </th>
                                                    <th>
                                                        @Asset.AssetCode
                                                    </th>
                                                    <th>
                                                        @Asset.AssetName
                                                    </th>
                                                    <th>
                                                        @Asset.ItemName
                                                    </th>
                                                    <th>
                                                        @Asset.SupplierName
                                                    </th>
                                                    <th>
