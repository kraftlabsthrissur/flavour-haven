@model IEnumerable<TradeSuiteApp.Web.Areas.Masters.Models.SerialNumberModel>
@{ 
        ViewBag.Title = "Index";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.SerialNumber</h1>
            <span class="uk-text-upper uk-text-small">@Masters.SerialNumberList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Edit", Html.ActionLink(Common.Edit, "Edit", "SerialNumber", new { Area = "Masters" }, new { @class = "md-btn" }))
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <table class="uk-table" id="serial-number-list" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th class="width-20">@Common.SerialNo</th>
                            <th>@Masters.SerialNumberFor</th>
                            <th class="width-80">@Masters.Prefix</th>
                            <th class="width-80">@Masters.LocationPrefix</th>
                            <th class="width-80">@Masters.FinYearPrefix</th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th> @Common.SerialNo</th>
                            <th >@Masters.SerialNumberFor</th>
                            <th >@Masters.Prefix</th>
                            <th >@Masters.LocationPrefix</th>
                            <th >@Masters.FinYearPrefix</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/SerialNumber")
<script type="text/javascript">
    serialNumber.list();
</script>

