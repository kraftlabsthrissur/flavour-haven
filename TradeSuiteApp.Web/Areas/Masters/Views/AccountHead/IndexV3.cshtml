@model IEnumerable<TradeSuiteApp.Web.Areas.Masters.Models.AccountGroupModel>
@{
    ViewBag.Title = Masters.AccountHeadList;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.AccountHead</h1>
            <span class="uk-text-upper uk-text-small">@Masters.AccountHeadList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("CreateV3", Html.ActionLink(Common.Create, "CreateV3", "AccountHead", new { Area = "Masters" }, new { @class = "md-btn" }))
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab hai" data-uk-tab="{connect:'#tabs-account-head',animation:'slide-horizontal'}" id="tabs-accounthead">
                    <li class="uk-active" data-tab="accounthead"><a>@Masters.AccountHead</a></li>
                </ul>
                <ul id="tabs-account-head" class="uk-switcher uk-margin">
                    <li>
                        <table class="uk-table" id="account_head_list" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-40">@Masters.AccountID</th>
                                    <th>@Masters.AccountName</th>
                                    <th>@Masters.AccountGroup</th>
                                    <th class="width-40"></th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th>@Masters.AccountID</th>
                                    <th>@Masters.AccountName</th>
                                    <th>@Masters.AccountGroup</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/AccountHead")
<script type="text/javascript">
    AccountHead.list();
    AccountHead.init();
</script>

