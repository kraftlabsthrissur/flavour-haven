@model TradeSuiteApp.Web.Areas.Masters.Models.DACalculationsModel

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.DACalculations</h1>
            <span class="uk-text-upper uk-text-small">@Masters.CreateNewDACalculations</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn BtnSave'>" + Common.Save + "</a>")
            

            @Html.ActionLink(Common.Close, "Index", "DACalculations", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="" class="form-view">
        <div class="md-card uk-margin-medium-bottom remove-box-shadow">
            <div class="md-card-content">
                <div class="uk-grid" data-uk-grid-margin>

                    @{Html.RenderPartial("/Areas/Masters/Views/DACalculations/Form.cshtml", Model); }

                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/DACalculation")
<script type="text/javascript">
    DA.init();
</script>

