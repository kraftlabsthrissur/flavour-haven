@model TradeSuiteApp.Web.Areas.Masters.Models.ManufacturerModel
@{
    ViewBag.Title = "Form";
}
<div class="section-group">
    <div class="uk-form-row">
        <div class="uk-width-medium-1-1">
            <div class="uk-grid" data-uk-grid-margin="">
                <div class="uk-width-medium-1-10">
                    <label>@Masters.Code</label>
                    @Html.TextBoxFor(m => m.Code, new { @class = "md-input label-fixed", @disabled = "disabled" })
                </div>
                <div class="uk-width-medium-1-6">
                    <label>@Masters.Name<span>*</span></label>
                    @Html.TextBoxFor(m => m.Name, new { @class = "md-input label-fixed" })
                </div>
                <div class="uk-width-medium-1-6">
                    <label>@Masters.AddressLine1<span>*</span></label>
                    @Html.TextBoxFor(m => m.AddressLine1, new { @class = "md-input label-fixed" })
                </div>
                <div class="uk-width-medium-1-6">
                    <label>@Masters.AddressLine2</label>
                    @Html.TextBoxFor(m => m.AddressLine2, new { @class = "md-input label-fixed" })
                </div>
                
            </div>
        </div>
    </div>
</div>
<div class="section-group">
    <div class="uk-form-row">
        <div class="uk-width-medium-1-1">
            <div class="uk-grid" data-uk-grid-margin="">
                <div class="uk-width-medium-1-10">
                    <label>@Masters.State<span>*</span></label>
                    @Html.DropDownListFor(Model => Model.StateID, Model.StateList, "Select", new { @class = "md-input label-fixed" })
                </div>
                <div class="uk-width-medium-1-6">
                    <label>@Masters.Place<span>*</span></label>
                    @Html.TextBoxFor(m => m.Place, new { @class = "md-input label-fixed" })
                </div>
                <div class="uk-width-medium-1-6">
                    <div class="uk-input-group">
                        <label>@Masters.Phone*</label>
                        @Html.TextBoxFor(m => m.Phone, new { @class = "md-input label-fixed" })
                    </div>
                </div>
                <div class="uk-width-medium-1-6">
                    <label>@Masters.Description</label>
                    @Html.TextBoxFor(m => m.Description, new { @class = "md-input label-fixed" })
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/Manufacturer")
<script type="text/javascript">
    Manufacturer.init();
</script>






