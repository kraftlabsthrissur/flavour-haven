@model TradeSuiteApp.Web.Areas.Masters.Models.MouldModel

@{
    ViewBag.Title = "Mould";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.Mould</h1>
            <span class="uk-text-upper uk-text-small">@Masters.EditMould</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @Html.HiddenFor(m => m.ID)
            @Html.ActionLink(Common.Close, "Index", "Mould", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="" class="form-view">
        <div class="md-card uk-margin-medium-bottom remove-box-shadow">
            <div class="md-card-content">
                <div class="uk-grid" data-uk-grid-margin>
                    @{Html.RenderPartial("/Areas/Masters/Views/Mould/Form.cshtml", Model); }
                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("~/Areas/Masters/Views/Item/StockRequestItemList.cshtml")
@Scripts.Render("~/Js/Page/Item")

@Scripts.Render("~/Js/Page/Mould")
<script type="text/javascript">
    Mould.init();
</script>


