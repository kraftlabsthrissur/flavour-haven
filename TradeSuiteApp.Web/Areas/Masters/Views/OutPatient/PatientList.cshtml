
@{
    ViewBag.Title = "PartialView";
}


<div class="uk-modal" id="select-patient" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Masters.SelectPatient<i class="material-icons"></i></h3>
        </div>
        <table id="patient-list" width="100%" class="uk-table" cellspacing="0">
            <thead>
                <tr>
                    <th class="width-60">@Common.SerialNo</th>
                    <th class="width-30"></th>
                    <th class="width-100">@Masters.Code</th>
                    <th>@Masters.Name</th>
                    <th>@Masters.Place</th>
                    <th>@Masters.MobileNo</th>
                </tr>

            </thead>
            <thead class="search">
                <tr>
                    <th class="width-60">@Common.SerialNo</th>
                    <th class="width-30"></th>
                    <th class="width-100">@Masters.Code</th>
                    <th>@Masters.Name</th>
                    <th>@Masters.Place</th>
                    <th>@Masters.MobileNo</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKPatient">Ok</button>
        </div>
    </div>
</div>



