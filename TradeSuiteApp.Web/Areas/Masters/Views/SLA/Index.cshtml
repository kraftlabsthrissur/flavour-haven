@model TradeSuiteApp.Web.Areas.Masters.Models.SLAModel
@{
    ViewBag.Title = "SLA";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.SLA</h1>
            <span class="uk-text-upper uk-text-small">@Masters.SLAList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "SLA", new { Area = "Masters" }, new { @class = "md-btn" }))
            
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="uk-grid" data-uk-grid-margin>
                    <div class="uk-width-medium-1-1">
                        <div class="uk-grid" data-uk-grid-margin>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.ProcessCycle</label>
                                @Html.DropDownList("Cycle", new SelectList(Model.ProcessCycleList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.TransactionType</label>
                                @Html.DropDownList("TransactionType", new SelectList(Model.TransactionTypeList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.SLAKeyValue</label>
                                @Html.DropDownList("KeyValue", new SelectList(Model.SLAKeyValueList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.Item</label>
                                @Html.DropDownList("SLAItem", new SelectList(Model.ItemList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.Suppliers</label>
                                @Html.DropDownList("SLASupplier", new SelectList(Model.SupplierList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                            <div class="uk-width-medium-1-6">
                                <label>@Masters.Customers</label>
                                @Html.DropDownList("SLACustomer", new SelectList(Model.CustomerList, "Value", "Text"), "Select", new { @class = "md-input label-fixed" })
                            </div>
                        </div>
                    </div>
                    <div class="uk-width-medium-1-1">
                        <div id="wrapper" style="width:100%; overflow-x:auto;">
                            <table id="tbl-sla-list" class="uk-table" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th class="width-30">@Common.SerialNo</th>
                                        <th class="width-60">@Masters.ProcessCycle</th>
                                        <th class="width-60">@Masters.TransactionType</th>
                                        <th class="width-60">@Masters.SLAKeyValue</th>
                                        <th class="width-60">@Masters.Item</th>
                                        <th class="width-60">@Masters.Suppliers</th>
                                        <th class="width-60">@Masters.Customers</th>
                                        <th class="width-60">@Masters.ItemAccountCategory</th>
                                        <th class="width-60">@Masters.ItemTaxCategory</th>
                                        <th class="width-60">@Masters.BatchPrefix</th>
                                        <th class="width-60">@Masters.SupplierAccountCategory</th>
                                        <th class="width-60">@Masters.SupplierTaxCategory</th>
                                        <th class="width-60">@Masters.SupplierTaxSubCategory</th>
                                        <th class="width-60">@Masters.CustomerTaxCategory</th>
                                        <th class="width-60">@Masters.CustomerAccountCategory</th>
                                        <th class="width-60">@Masters.CostComponent</th>
                                        <th class="width-60">@Masters.CustomerCategory</th>
                                        <th class="width-60">@Masters.Capitilization</th>
                                        <th class="width-60">@Masters.DepartmentCategory</th>
                                        <th class="width-60">Debit @Masters.AccountNumber</th>
                                        <th class="width-60">@Masters.DebitAccountDescription</th>
                                        <th class="width-60">Credit @Masters.AccountNumber</th>
                                        <th class="width-60">@Masters.CreditAccountDescription</th>

                                    </tr>
                                </thead>
                                <thead class="search">
                                    <tr>
                                        <th>@Common.SerialNo</th>
                                        <th>@Masters.ProcessCycle</th>
                                        <th>@Masters.TransactionType</th>
                                        <th>@Masters.SLAKeyValue</th>
                                        <th>@Masters.Item</th>
                                        <th>@Masters.Suppliers</th>
                                        <th>@Masters.Customers</th>
                                        <th>@Masters.ItemAccountCategory</th>
                                        <th>@Masters.ItemTaxCategory</th>
                                        <th>@Masters.BatchPrefix</th>
                                        <th>@Masters.SupplierAccountCategory</th>
                                        <th>@Masters.SupplierTaxCategory</th>
                                        <th>@Masters.SupplierTaxSubCategory</th>
                                        <th>@Masters.CustomerTaxCategory</th>
                                        <th>@Masters.CustomerAccountCategory</th>
                                        <th>@Masters.CostComponent</th>
                                        <th>@Masters.CustomerCategory</th>
                                        <th>@Masters.Capitilization</th>
                                        <th>@Masters.DepartmentCategory</th>
                                        <th>Debit @Masters.AccountNumber</th>
                                        <th>@Masters.DebitAccountDescription</th>
                                        <th>Credit @Masters.AccountNumber</th>
                                        <th>@Masters.CreditAccountDescription</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                       
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/SLAMaster")
<script type="text/javascript">
    SLA.list();
    SLA.init();
</script>
