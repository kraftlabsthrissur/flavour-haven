@model TradeSuiteApp.Web.Areas.Masters.Models.UserRolesModel
@{
    ViewBag.Title = "Create UserRoles";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.UserRoles</h1>
            <span class="uk-text-upper uk-text-small">@Masters.CreateUserRoles</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @Html.ActionLink(Common.Close, "Index", "UserRoles", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="sales form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("Form", Model); }
            </div>
        </div>
    </div>
</div>

@Html.Partial("~/Areas/Masters/Views/Employee/PartialList.cshtml")
@Html.Partial("~/Areas/Masters/Views/UserRoles/PartialList.cshtml")
@Scripts.Render("~/Js/Page/UserRole")
@Scripts.Render("~/Js/Page/Item")
@Scripts.Render("~/Js/Page/Employee")

<script type="text/javascript">
    UserRole.init();
</script>
