<div class="uk-modal" id="select-item" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Purchase.SelectItems<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="item-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@Purchase.Code</th>
                    <th class="width-100">@Purchase.ItemName</th>
                    <th class="width-100">@Masters.PartsNo</th>
                    <th class="width-100">@Masters.Remarks</th>
                    <th class="width-100">@Masters.Model</th>
                    <th class="width-60">@Purchase.Unit</th>
                    <th class="width-60">@Purchase.ItemCategory</th>
                    <th class="width-60">@Purchase.PurchaseCategory</th>
                    <th class="width-60">@Purchase.PendingOrderQty</th>
                    <th class="width-60">@Purchase.QuantityAvailable (Stock)</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>@Common.SerialNo</th>
                    <th></th>
                    <th>@Purchase.Code</th>
                    <th>@Purchase.ItemName</th>
                    <th>@Masters.PartsNo</th>
                    <th>@Masters.Remarks</th>
                    <th>@Masters.Model</th>
                    <th>@Purchase.Unit</th>
                    <th>@Purchase.ItemCategory</th>
                    <th>@Purchase.PurchaseCategory</th>
                    <th>@Purchase.PendingOrderQty</th>
                    <th>@Purchase.QuantityAvailable</th>

                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKItem">Ok</button>
        </div>
    </div>
</div>



