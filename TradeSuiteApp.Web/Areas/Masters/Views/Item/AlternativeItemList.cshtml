<div class="uk-modal" id="select-alternative-item" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Masters.SelectAlternativeItem<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="alternative-item-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-60">@Common.SerialNo</th>
                    <th class="width-30"></th>
                    <th class="width-40">@Masters.Code</th>
                    <th class="">@Masters.Name</th>
                    <th class="width-120">@Masters.Category</th>
                    <th class="width-120">@Masters.Unit</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>@Common.SerialNo</th>
                    <th></th>
                    <th>@Masters.Code</th>
                    <th>@Masters.Name</th>
                    <th>@Masters.Category</th>
                    <th>@Masters.Unit</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnAddAlternativeItem">Add</button>
        </div>
    </div>
</div>



