<div class="uk-modal" id="select-item" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Purchase.SelectItems<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="item-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@Sales.Code</th>
                    <th class="">@Sales.ItemName</th>
                    <th class="">@Sales.Unit</th>
                    <th class="">@Sales.ItemCategory</th>
                    <th class="">@Sales.SalesCategory</th>

                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>@Common.SerialNo</th>
                    <th></th>
                    <th>@Sales.Code</th>
                    <th>@Sales.ItemName</th>
                    <th>@Sales.Unit</th>
                    <th>@Sales.ItemCategory</th>
                    <th class="">@Sales.SalesCategory</th>

                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn md-btn-flat uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-flat uk-modal-close" id="btnOKItem">Ok</button>
        </div>
    </div>
</div>



