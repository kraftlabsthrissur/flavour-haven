
@{
    ViewBag.Title = "PartialView";
}


<div class="uk-modal" id="select-production-issue-item" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Manufacturing.SelectReceiptItem<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="production-issue-item-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-20">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@Manufacturing.Code</th>
                    <th>@Manufacturing.ItemName</th>
                    <th class="width-60">@Manufacturing.Batchsize</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th class="width-20">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@Manufacturing.Code</th>
                    <th>@Manufacturing.ItemName</th>
                    <th class="width-60">@Manufacturing.Batchsize</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKIssueItem">Ok</button>
        </div>
    </div>
</div>



