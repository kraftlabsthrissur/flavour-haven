
@{
    ViewBag.Title = "PartialView";
}


<div class="uk-modal" id="select-supplier" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Purchase.SelectSupplier<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="supplier-list" class="uk-table" cellspacing="0" >
            <thead>
                <tr>
                    <th class="width-10">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-40">@Purchase.Code</th>
                    <th class="width-200">@Purchase.SupplierName</th>
                    <th class="width-120">@Purchase.SupplierLocation</th>
                    <th class="width-120">@Purchase.SupplierCategory</th>
                    <th class="width-120">@Masters.Landline</th>
                    <th class="width-120">@Masters.MobileNo</th>
                    <th class="width-120">@Purchase.ItemCategory</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>@Common.SerialNo</th>
                    <th></th>
                    <th>@Purchase.Code</th>
                    <th>@Purchase.SupplierName</th>
                    <th>@Purchase.Location</th>
                    <th>@Masters.Landline</th>
                    <th>@Masters.MobileNo</th>
                    <th>@Purchase.SupplierCategory</th>
                    <th>@Purchase.ItemCategory</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKSupplier">Ok</button>
        </div>
    </div>
</div>


