
@{
    ViewBag.Title = "CustomerItemDetails";
}

<div class="uk-modal" id="select-customer-items" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Masters.CustomerItemDetails<i class="material-icons"></i></h3>
        </div>
       
        <table id="customer-item-details-list" class="uk-table">
            <thead>
                <tr>
                    <th class="width-20">@Common.SerialNo</th>
                    <th class="width-50">@Masters.Code</th>
                    <th class="width-200">@Masters.ItemName</th>
                    <th class="width-50">@Masters.MRP</th>
                    <th class="width-30">@Masters.Discount%</th>
                    <th class="width-50">@Masters.Quantity</th>
                    <th class="width-30">@Masters.OfferQuantity</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th class="width-20">@Common.SerialNo</th>
                    <th class="width-50">@Masters.Code</th>
                    <th class="width-200">@Masters.ItemName</th>
                    <th class="width-50">@Masters.MRP</th>
                    <th class="width-30">@Masters.Discount%</th>
                    <th class="width-50">@Masters.Quantity</th>
                    <th class="width-30">@Masters.OfferQuantity</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
        </div>
    </div>
</div>
  
@Scripts.Render("~/Js/Page/Customer")
<script type="text/javascript">
    Customer.customer_item_details_list();
</script>






