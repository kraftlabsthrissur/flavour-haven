@model TradeSuiteApp.Web.Areas.Masters.Models.ApprovalFlowModel
@{
    ViewBag.Title = "Create Approval FLow";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_title">
            <h1>@Approvals.ApprovalFlow</h1>
            <span class="uk-text-upper uk-text-small">@Approvals.CreateApprovalFlow</span>
        </div>
        <div class="heading_actions">
            <a class="md-btn btnSave">@Common.Save</a>
            <a class="md-btn btnSaveNew">@Common.SaveAndNew</a>
            @Html.ActionLink(Common.Close, "Index", "ApprovalFlow", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a href="#">@Common.Action</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Masters/Views/ApprovalFlow/_form.cshtml", Model); }

            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/ApprovalFlow")
<script type="text/javascript">
    ApprovalFlow.init();
</script>