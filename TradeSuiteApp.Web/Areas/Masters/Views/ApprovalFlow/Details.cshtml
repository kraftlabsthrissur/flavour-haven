@model TradeSuiteApp.Web.Areas.Masters.Models.ApprovalFlowModel
@{
    ViewBag.Title = "Approval FLow Detail";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Approvals.ApprovalFlow</h1>
            <span class="uk-text-upper uk-text-small">@Approvals.ApprovalFlowDetails</span>
        </div>
        <div class="heading_actions">
            @Html.ActionLink(Common.Edit, "Edit", "ApprovalFlow", new { Area = "Masters", ID = Model.ID }, new { @class = "md-btn Edit" })
            @Html.ActionLink(Common.Close, "Index", "ApprovalFlow", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>

    </div>

    <div id="page_content_inner">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    <div class="uk-form-row">
                        <div class="uk-grid" data-uk-grid-margin="">
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.Location</label>
                                @Html.TextBoxFor(m => m.LocationName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                           
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.ForDepartment</label>
                                @Html.TextBoxFor(m => m.ForDepartmentName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.AmountAbove</label>
                                @Html.TextBoxFor(m => m.AmountAbove, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.AmountBelow</label>
                                @Html.TextBoxFor(m => m.AmountBelow, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                        </div>
                        <br />
                    </div>
                    <div class="uk-form-row">
                        <div class="uk-grid" data-uk-grid-margin="">

                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.ItemCategory</label>
                                @Html.TextBoxFor(m => m.ItemCategoryName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.ItemAccountsCategory</label>
                                @Html.TextBoxFor(m => m.ItemAccountsCategoryName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.ApprovalFlow</label>
                                @Html.TextBoxFor(m => m.AppQueueName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="uk-form-row">
                        <div class="uk-grid" data-uk-grid-margin="">
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.SupplierCategory</label>
                                @Html.TextBoxFor(m => m.SuppliercategoryName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                            <div class="uk-width-medium-2-10">
                                <label>@Approvals.SupplierAccountsCategory</label>
                                @Html.TextBoxFor(m => m.SupplierAccountscategoryName, new { @class = "md-input", @disabled = "disabled" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

