
@model TradeSuiteApp.Web.Areas.Masters.Models.AgeingBucketModel

@{
    ViewBag.Title = Masters.EditAgeingBucket;
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.AgeingBucket</h1>
            <span class="uk-text-upper uk-text-small">@Masters.EditAgeingBucket</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @Html.HiddenFor(m => m.ID)
            @Html.ActionLink(Common.Close, "Index", "AgeingBucket", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Masters/Views/AgeingBucket/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>


@Scripts.Render("~/Js/Page/AgeingBucket")
<script type="text/javascript">
    AgeingBucket.init();
</script>


