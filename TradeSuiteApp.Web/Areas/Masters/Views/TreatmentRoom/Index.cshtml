@{
    ViewBag.Title = Masters.TreatmentRoomList;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.TreatmentRoom</h1>
            <span class="uk-text-upper uk-text-small">@Masters.TreatmentRoomList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "TreatmentRoom", new { Area = "Masters" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <table class="uk-table" id="treatmentroom-list" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th class="width-20">@Common.SerialNo</th>
                            <th>@Masters.TreatmentRoomName</th>
                            <th>@Masters.Remarks</th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th class="width-20">@Common.SerialNo</th>
                            <th>@Masters.TreatmentRoomName</th>
                            <th>@Masters.Remarks</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/TreatmentRoom")
<script type="text/javascript">
    TreatmentRoom.list();
</script>




