<div class="uk-modal" id="select-appoinment-scheduled-patient" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Select Patient<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="appoinment-scheduled-patient-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>

                    <th class="width-30">
                        @Common.SerialNo
                    </th>
                    <th class="width-10"></th>
                    <th class="width-60">
                        @Masters.HIN
                    </th>
                    <th>
                        @Masters.PatientName
                    </th>
                    <th class="width-60">
                        @Masters.OPNo
                    </th>
                    <th class="width-60">
                        @Masters.OPDate
                    </th>                    
                    <th class="width-60">
                        @Masters.Phone
                    </th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>
                        @Common.SerialNo
                    </th>
                    <th></th>
                    <th class="width-60">
                        @Masters.PatientCode
                    </th>
                    <th>
                        @Masters.PatientName
                    </th>
                    <th class="width-60">
                        @Masters.OPNo
                    </th>
                    <th class="width-60">
                        @Masters.OPDate
                    </th>
                    <th class="width-60">
                        @Masters.Phone
                    </th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn  uk-modal-close">@Common.Close</button>
            <button type="button" class="md-btn md-btn-primary" id="btnOkAddPatient">Ok</button>
        </div>
    </div>
</div>


