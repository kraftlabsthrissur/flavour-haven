<div class="uk-modal" id="select-ip-patient" aria-hidden="true">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Masters.SelectPatient<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="in-patient-list" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@AHCMS.PatientCode</th>
                    <th class="">@AHCMS.Patient</th>
                    <th class="width-60">@AHCMS.IPNO</th>
                    <th class="width-120">@AHCMS.AdmissionDate</th>  
                    <th class="width-120">@AHCMS.Room</th>                
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-60">@AHCMS.PatientCode</th>
                    <th class="">@AHCMS.Patient</th>
                    <th class="width-60">@AHCMS.IPNO</th>
                    <th class="width-120">@AHCMS.AdmissionDate</th>
                    <th class="width-120">@AHCMS.Room</th>           
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKItem">Ok</button>
        </div>
    </div>
</div>



