@model TradeSuiteApp.Web.Areas.Masters.Models.OutPatientModel
@{
    ViewBag.Title = "Patient Details";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.Patient</h1>
            <span class="uk-text-upper uk-text-small">@Masters.PatientDetails</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Edit", Html.ActionLink(Common.Edit, "Edit", "InternationalPatient", new { Area = "Masters", id = (Model != null) ? Model.ID : 0 }, new { @class = "md-btn" }))
            @Html.HiddenFor(m => m.ID)
            @Html.ActionLink(Common.Close, "Index", "InternationalPatient", new { Area = "Masters" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="details-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    <div class="uk-width-medium-8-10">
                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin="">
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.HIN</label>
                                    @Html.TextBoxFor(m => m.Code, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.FirstName<span>*</span></label>
                                    @Html.TextBoxFor(m => m.Name, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.MiddleName</label>
                                    @Html.TextBoxFor(m => m.MiddleName, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.LastName</label>
                                    @Html.TextBoxFor(m => m.LastName, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label for="uk_dp_start">@Masters.Doctor</label>
                                    @Html.TextBoxFor(model => model.DoctorName, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.GuardianName</label>
                                    @Html.TextBoxFor(m => m.GuardianName, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Gender <span>*</span></label>
                                    @Html.TextBoxFor(m => m.Gender, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.MaritalStatus </label>
                                    @Html.TextBoxFor(m => m.MartialStatus, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.BloodGroup</label>
                                    @Html.TextBoxFor(m => m.BloodGroup, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                              
                            </div>
                        </div>

                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin="">
                                <div class="uk-width-medium-1-8">
                                    <div class="uk-input-group">
                                        <label>@Masters.DOB<span>*</span></label>
                                        @Html.TextBoxFor(m => m.DOB, new { @class = "md-input label-fixed  past-date date", @disabled = "disabled" })
                                        <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                    </div>
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Age</label>
                                    @Html.TextBoxFor(m => m.Age, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Month</label>
                                    @Html.TextBoxFor(m => m.Month, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.AddressLine1<span>*</span></label>
                                    @Html.TextBoxFor(m => m.AddressLine1, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.AddressLine2</label>
                                    @Html.TextBoxFor(m => m.AddressLine2, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                            </div>
                        </div>

                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin="">
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Country</label>
                                    @Html.TextBoxFor(m => m.Country, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.States</label>
                                    @Html.TextBoxFor(m => m.State, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.District</label>
                                    @Html.TextBoxFor(m => m.District, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.PIN</label>
                                    @Html.TextBoxFor(m => m.PinCode, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Place</label>
                                    @Html.TextBoxFor(m => m.Place, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.PatientReferedBy</label>
                                    @Html.TextBoxFor(m => m.PatientReferedBy, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                            </div>
                        </div>
                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin="">
                             
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.ReferalName</label>
                                    @Html.TextBoxFor(m => m.ReferalName, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.ReferalContactNo</label>
                                    @Html.TextBoxFor(m => m.ReferalContactNo, new { @class = "md-input label-fixed", @maxlength = "30", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-4">
                                    <label>@Masters.EmergencyContactNo.<span>*</span></label>
                                    @Html.TextBoxFor(m => m.EmergencyContactNo, new { @class = "md-input label-fixed", @maxlength = "30", @disabled = "disabled" })
                                </div>
                                <div class="uk-width-medium-1-8">
                                    <label>@Masters.Occupation</label>
                                    @Html.TextBoxFor(m => m.Occupation, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                </div>
                                </div>
                            </div>
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-8">
                                            <label>@Masters.CountryCode<span>*</span></label>
                                            @Html.TextBoxFor(m => m.CountryCode, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>@Masters.MobileNo<span>*</span></label>
                                            @Html.TextBoxFor(m => m.MobileNumber, new { @class = "md-input label-fixed", @maxlength = "30", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>@Masters.Email</label>
                                            @Html.TextBoxFor(m => m.Email, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <label>@Masters.Landline</label>
                                            @Html.TextBoxFor(m => m.LandLine, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>

                                    </div>
                                </div>

                                <hr class="international" />
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-4 international">
                                            <label>@Masters.Purposeofvisit</label>
                                            @Html.TextBoxFor(m => m.PurposeOfVisit, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-4 international">
                                            <label>@Masters.PassportNo</label>
                                            @Html.TextBoxFor(m => m.PassportNo, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <label>@Masters.PlaceOfIssue</label>
                                            @Html.TextBoxFor(m => m.PlaceOfIssue, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start">@Masters.DateOfIssue</label>
                                                @Html.TextBoxFor(m => m.DateOfIssuePassport, new { @class = "md-input label-fixed  past-date date", @disabled = "disabled" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start">@Masters.DateOfExpiry</label>
                                                @Html.TextBoxFor(m => m.DateOfExpiry, new { @class = "md-input label-fixed  future-date date", @disabled = "disabled" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar future-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <label>@Masters.EmployedInIndia </label>
                                            @Html.TextBoxFor(m => m.EmployedIn, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                    </div>
                                </div>

                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin="">
                                        <div class="uk-width-medium-1-4 international">
                                            <label>@Masters.VisaNo</label>
                                            @Html.TextBoxFor(m => m.VisaNo, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <div class="uk-input-group">
                                                <label>@Masters.DateOfIssue (Visa)</label>
                                                @Html.TextBoxFor(m => m.DateOfIssueVisa, new { @class = "md-input label-fixed  past-date date", @disabled = "disabled" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <div class="uk-input-group">
                                                <label>@Masters.DateOfExpiry (Visa)</label>
                                                @Html.TextBoxFor(m => m.DateOfExpiryVisa, new { @class = "md-input label-fixed  future-date date", @disabled = "disabled" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar future-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-4 international">
                                            <label>@Masters.ArrivedFrom</label>
                                            @Html.TextBoxFor(m => m.ArrivedFrom, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-4 international">
                                            <label>@Masters.ProceedingTo</label>
                                            @Html.TextBoxFor(m => m.ProceedingTo, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <label>@Masters.DurationOfStay (days)</label>
                                            @Html.TextBoxFor(m => m.DurationOfStay, new { @class = "md-input label-fixed", @disabled = "disabled" })
                                        </div>
                                        <div class="uk-width-medium-1-8 international">
                                            <div class="uk-input-group">
                                                <label>@Masters.DateOfArrival</label>
                                                @Html.TextBoxFor(m => m.DateOfArrival, new { @class = "md-input label-fixed  past-date date", @disabled = "disabled" })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                            </div>
                                        </div>

                                        <div class="uk-width-medium-1-4">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>@Masters.Photo</label>
                                                <div class="uk-form-file v1">
                                                    <label id="selected-Photo">
                                                        @foreach (var item in Model.SelectedPhoto)
                                                        {
                                                            <span class='view-file' data-id='@item.ID' data-url='@item.URL' data-path='@item.Path'>@item.Name</span>
                                                        }
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-4">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>@Purchase.OtherQuotation</label>
                                                <div class="uk-form-file v1">
                                                    <div id="other-quotation-list">
                                                        <div class="uk-button-dropdown" data-uk-dropdown>
                                                            <a><span id="file-count">@Model.OtherQuotation.Count() Files</span> <i class="material-icons">&#xE313;</i></a>
                                                            <div class="uk-dropdown">
                                                                <ul class="uk-nav uk-nav-dropdown">
                                                                    @foreach (var item in Model.OtherQuotation)
                                                                    {
                                                                        <li class='file-list'>
                                                                            <span data-id='@item.ID' class='view-file' data-url='@item.URL' data-path='@item.Path'>
                                                                                @item.Name
                                                                            </span>
                                                                        </li>
                                                                    }
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-4 international">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>@Masters.PassportCopy</label>
                                                <div class="uk-form-file v1">
                                                    <label id="selected-passport">
                                                        @foreach (var item in Model.SelectedPassport)
                                                        {
                                                            <span class='view-file' data-id='@item.ID' data-url='@item.URL' data-path='@item.Path'>@item.Name</span>
                                                        }
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-4 international">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>@Masters.VisaCopy</label>
                                                <div class="uk-form-file v1">
                                                    <label id="selected-visa">
                                                        @foreach (var item in Model.SelectedVisa)
                                                        {
                                                            <span class='view-file' data-id='@item.ID' data-url='@item.URL' data-path='@item.Path'>@item.Name</span>
                                                        }
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/Supplier")
@Scripts.Render("~/Js/Page/InternationalPatient")
<script type="text/javascript">
    InternationalPatient.change_country_detail();
</script>