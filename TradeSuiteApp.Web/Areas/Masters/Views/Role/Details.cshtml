@model TradeSuiteApp.Web.Areas.Masters.Models.RoleModel
@{
    ViewBag.Title = "Details";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.Role</h1>
            <span class="uk-text-upper uk-text-small">@Masters.RoleDetails</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Edit", Html.ActionLink(Common.Edit, "edit", "Role", new { Area = "Masters", @id = Model.ID }, new { @class = "md-btn " }))
            @Html.ActionLink(Common.Close, "Index", "Role", new { Area = "Masters" }, new { @class = "md-btn " })
            @Html.HiddenFor(m => m.ID)
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
        </div>
    <div id="page_content_inner" class="sales details-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Masters/Views/Role/View.cshtml", Model); }
            </div>
        </div>
    </div>
    </div>

