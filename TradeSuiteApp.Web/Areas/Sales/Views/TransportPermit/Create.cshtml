





@model TradeSuiteApp.Web.Areas.Sales.Models.TransportPermitModel
@{
    ViewBag.Title = "Transport Permit";

}
<div id="page_content">

    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Sales.TransportPermitTitle</h1>
            <span class="uk-text-upper uk-text-small">@Sales.CreateTransportPermit</span>
        </div>
        <div class="heading_actions">

            @*<form action="/Reports/Sales/PickList/@Model.ID" method="POST">
                    @ActionAuthorization.IsAuthorized("Print", "<input type='submit' class='md-btn' value='Print' />")
                </form>*@
       
            @ActionAuthorization.IsAuthorized("SaveAndPrint", " <a class='md-btn SaveAndPrint'>Save And Print</a>")
            @Html.ActionLink(Common.Back, "Index", "TransportPermit", new { Area = "Sales" }, new { @class = "md-btn " })




            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>

    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    <div class="uk-form-row">
                        <div class="uk-grid" data-uk-grid-margin="">
                            <div class="uk-width-medium-3-5">
                                <form id="report-filter-form" method="POST" data-name="TransportPermitReport">
                                    <div class="uk-form-row ">
                                        <div class="uk-grid" data-uk-grid-margin="">
                                            <div class="uk-width-medium-1-5">
                                              
                                                    <label for="uk_dp_start">@Reports.TransNo</label>
                                                    @Html.TextBoxFor(model => model.TransNo, new { @class = "md-input label-fixed  " ,disabled="disabled"})
                                                 
                                                
                                            </div>
                                            <div class="uk-width-medium-1-5">
                                                <div class="uk-input-group">
                                                    <label for="uk_dp_start">@Reports.Fromdate</label>
                                                    @Html.TextBoxFor(model => model.FromDate, new { @class = "md-input label-fixed  past-date date" })
                                                    <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                                </div>
                                            </div>
                                            <div class="uk-width-medium-1-5">
                                                <div class="uk-input-group">
                                                    <label for="uk_dp_start">@Reports.Todate</label>
                                                    @Html.TextBoxFor(model => model.ToDate, new { @class = "md-input label-fixed  past-date date" })
                                                    <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </form>
                            </div>

                            <div class="uk-width-medium-7-10 ">
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin="" id="report-viewer">
                                    </div>
                                </div>
                            </div>

                            <div class="uk-width-medium-1-1">
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-8">
                                            <div class="uk-grid" data-uk-grid-margin>
                                                <label>&nbsp;</label>
                                            </div>
                                            <input type="checkbox" name="ReportType" class="StockTransferIssue" data-md-icheck checked value="StockTransferIssue" />&nbsp;@Html.Label("Stock Transfer Issue")
                                        </div>
                                        <div class="uk-width-medium-1-8 StockTransfer">
                                            <div class="uk-autocomplete " id="stock-issuenoFrom-autocomplete">
                                                <label for="uk_dp_start">@Reports.StockTransferNoFrom</label>
                                                @Html.TextBox("StockTransferNoFrom", "", new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
                                                    <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                        {{~items}}
                                                        <li data-value="{{ $item.IssueNo}}"
                                                            data-id="{{$item.ID}}">
                                                            <a>
                                                                {{ $item.IssueNo}}
                                                            </a>
                                                        </li>
                                                        {{/items}}
                                                    </ul>
                                                </script>
                                            </div>
                                            @Html.HiddenFor(m => m.StockTransferNoFromID)
                                        </div>

                                        <div class="uk-width-1-6 StockTransfer">
                                            <div class="uk-autocomplete " id="stock-issuenoTo-autocomplete">
                                                <label for="uk_dp_start">@Reports.StockTransferNoTo</label>
                                                @Html.TextBox("StockTransferNoTo", "", new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
                                                    <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                        {{~items}}
                                                        <li data-value="{{ $item.IssueNo}}"
                                                            data-id="{{$item.ID}}">
                                                            <a>
                                                                {{ $item.IssueNo}}
                                                            </a>
                                                        </li>
                                                        {{/items}}
                                                    </ul>
                                                </script>
                                            </div>
                                            @Html.HiddenFor(m => m.StockTransferNoToID)
                                        </div>


                                    </div>
                                </div>
                            </div>
                            <div class="uk-width-medium-1-1">
                                <div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-8">
                                            <div class="uk-grid" data-uk-grid-margin>
                                                <label>&nbsp;</label>
                                            </div>
                                            <input type="checkbox" name="ReportType" class="SalesInvoice" data-md-icheck checked value="SalesInvoice" />&nbsp;@Html.Label("Sales Invoice")
                                        </div>

                                        <div class="uk-width-medium-1-8 SalesInvoice">
                                            <div class="uk-autocomplete " id="sales-invoiceno_from-autocomplete">
                                                <label for="uk_dp_start">@Reports.SalesInvoiceNoFrom</label>
                                                @Html.TextBox("SalesInvoiceNoFrom", "", new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
                                                    <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                        {{~items}}
                                                        <li data-value="{{ $item.InvoiceNo}}"
                                                            data-id="{{$item.ID}}">
                                                            <a>
                                                                {{ $item.InvoiceNo}}
                                                            </a>
                                                        </li>
                                                        {{/items}}
                                                    </ul>
                                                </script>
                                            </div>
                                            @Html.HiddenFor(m => m.SalesInvoiceNoFromID)
                                        </div>
                                        <div class="uk-width-1-6 SalesInvoice">
                                            <div class="uk-autocomplete " id="sales-ordernoTo-autocomplete">
                                                <label for="uk_dp_start">@Reports.SalesInvoiceNoTo</label>
                                                @Html.TextBox("SalesInvoiceNoTo", "", new { @class = "md-input label-fixed" })
                                                <script type="text/autocomplete">
                                                    <ul class="uk-nav uk-nav-autocomplete uk-autocomplete-results">
                                                        {{~items}}
                                                        <li data-value="{{ $item.InvoiceNo}}"
                                                            data-id="{{$item.ID}}">
                                                            <a>
                                                                {{ $item.InvoiceNo}}
                                                            </a>
                                                        </li>
                                                        {{/items}}
                                                    </ul>
                                                </script>
                                            </div>
                                            @Html.HiddenFor(m => m.SalesInvoiceNoToID)
                                        </div>
                                        <div class="md-input-wrapper md-input-filled uk-width-medium-1-8">
                                            <label>&nbsp;</label>
                                            <button class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="Show">@Common.Show</button>
                                            @*<button type="submit" class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="report-filter-submit">Show</button>*@
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="uk-width-medium-1-1">
                                <div class="uk-form-row ">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-8">
                                            <label>@Reports.DriverName<span>*</span></label>
                                            @Html.TextBoxFor(m => m.DriverName, new { @class = "md-input label-fixed" })
                                        </div>
                                        <div class="uk-width-medium-1-8">
                                            <label>@Reports.VehicleNumber<span>*</span></label>
                                            @Html.TextBoxFor(m => m.VehicleNumber, new { @class = "md-input label-fixed" })
                                        </div>
                                        <div class="uk-width-medium-1-5">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start">@Sales.ValidFromdate</label>
                                                @Html.TextBoxFor(model => model.ValidFromDate, new { @class = "md-input label-fixed   all-date " })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar all-date"></i></span>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-5">
                                            <div class="uk-input-group">
                                                <label for="uk_dp_start">@Sales.ValidTodate</label>
                                                @Html.TextBoxFor(model => model.ValidToDate, new { @class = "md-input label-fixed  all-date " })
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar all-date"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                        <br>
                        <div class="uk-form-row">
                            <div class="uk-grid" data-uk-grid-margin>
                                <div class="uk-width-medium-1-1">
                                    <div>
                                        <table id="TransportPermitList" class="uk-table " cellspacing="0" width=100">
                                            <thead>
                                                <tr>
                                                    <th class="width-10">@Common.SerialNo</th>
                                                    <th class="width-10"><input type="checkbox" class="icheckbox select-all" data-md-icheck /></th>
                                                    <th class="width-200">@Sales.TransNo</th>
                                                    <th class="width-200">@Sales.TransDate</th>
                                                    <th class="width-250">@Sales.DocumentType</th>
                                                    <th class="width-250">@Sales.CustomerName</th>
                                                   
                                                    <th class="width-100">@Sales.District </th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>
                            <input type="hidden" id="item-count">
                            @*<div class="uk-form-row">
                                    <div class="uk-grid" data-uk-grid-margin>
                                        <div class="uk-width-medium-1-2">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>&nbsp;</label>
                                                <button type="submit" class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="report-filter-submit">Show</button>
                                            </div>
                                        </div>
                                        <div class="uk-width-medium-1-2">
                                            <div class="md-input-wrapper md-input-filled">
                                                <label>&nbsp;</label>
                                                <a class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light " id="Refresh">Reset</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>*@

                            @*</div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/TransportPermitReport")
<script type="text/javascript">
        TransportPermitReport.init();
</script>

