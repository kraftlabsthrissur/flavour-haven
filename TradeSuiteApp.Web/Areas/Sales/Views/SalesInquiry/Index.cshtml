                        <table id="sales-inquiry-draft-list" class="uk-table">
                            <thead>
                                <tr>
                                    <th class="width-40">@Common.SerialNo</th>
                                    <th class="width-80">@Sales.SalesInquiryNo</th>
                                    <th class="width-80">@Sales.SalesInquiryDate</th>
                                    <th class="width-200">@Sales.RequestedCustomerName</th>
                                    <th class="width-200">@Sales.PhoneNumber</th>
                                    <th class="width-200">@Sales.RequestedCustomerAddress</th>
                                    <th class="width-200">@Sales.Remarks</th>
                                    <th class="width-80">@Sales.GrossAmount</th>
                                    <th class="width-80">@Sales.NetAmount</th>
                                </tr>
                            </thead>
                            <thead class="search">
