@{
    ViewBag.Title = "Sales Inquiry";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Sales.SalesInquiry</h1>
            <span class="uk-text-upper uk-text-small">@Sales.SalesInquiry</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "SalesInquiry", new { Area = "Sales" }, new { @class = "md-btn" }))
            <input type="hidden" id="normalclass" value="@ViewBag.normalclass" />
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-sales-inquiryr-content',animation:'slide-horizontal'}" id="tabs-sales-order">
                    <li class="uk-active" data-tab="Draft"><a>@Sales.Draft</a></li>
                    <li data-tab="SalesInquiry"><a>@Sales.SalesInquiry</a></li>
                    <li data-tab="Converted"><a>Converted</a></li>
                    <li data-tab="Cancelled"><a>Cancelled</a></li>
                </ul>
                <ul id="tabs-sales-inquiryr-content" class="uk-switcher uk-margin">
