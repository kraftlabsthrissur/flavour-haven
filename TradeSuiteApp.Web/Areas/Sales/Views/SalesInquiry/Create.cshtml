@model TradeSuiteApp.Web.Areas.Sales.Models.SalesInquiryModel

@{
    ViewBag.Title = "Create Sales Inquiry";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }" class="uk-text-right">
        <div class="heading_title">
            <h1>@Sales.SalesInquiry</h1>
            <span class="uk-text-upper uk-text-small">@Sales.CreateSalesInquiry</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveASDraft'>" + Common.SaveAsDraft + "</a>")
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @Html.ActionLink(Common.Close, "Index", "SalesInquiry", new { Area = "Sales" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="sales form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Sales/Views/SalesInquiry/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Scripts.Render(("~/Js/Page/SalesInquiry"))
<script type="text/javascript">
    SalesInquiry.init();
</script>