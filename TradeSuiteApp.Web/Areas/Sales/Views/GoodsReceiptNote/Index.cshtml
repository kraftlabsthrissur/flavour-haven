@{
    ViewBag.Title = "Goods Delivery Note";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Sales.GoodsDeliveryNote</h1>
            <span class="uk-text-upper uk-text-small">@Sales.GoodsDeliveryNote</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "GoodsReceiptNote", new { Area = "Sales" }, new { @class = "md-btn" }))
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="sales list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-goods-receipt-content',animation:'slide-horizontal'}" id="tabs-goods-receipt">
                    <li class="uk-active" data-tab="Draft"><a>@Sales.Draft</a></li>
                    <li data-tab="GoodsReceipt"><a>@Sales.GoodsDeliveryNote</a></li>
                    <li data-tab="Cancelled"><a>@Sales.Cancelled</a></li>
                </ul>
                <ul id="tabs-goods-receipt-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="goods-receipt-draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Sales.TransNo</th>
                                    <th class="width-80">@Sales.TransDate</th>
                                    @*<th class="width-80">@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th class="width-120">@Sales.CustomerLocation</th>*@
                                    <th class="width-80">@Sales.NetAmount</th>
                                   
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Sales.InvoiceNo</th>
                                    <th>@Sales.InvoiceDate</th>
                                    @*<th>@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th>@Sales.CustomerLocation</th>*@
                                    <th>@Sales.NetAmount</th>
                                    
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="goods-receipt-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Sales.TransNo</th>
                                    <th class="width-80">@Sales.TransDate</th>
                                    @*<th class="width-80">@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th class="width-120">@Sales.CustomerLocation</th>*@
                                    <th class="width-80">@Sales.NetAmount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Sales.InvoiceNo</th>
                                    <th>@Sales.InvoiceDate</th>
                                    @*<th>@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th>@Sales.CustomerLocation</th>*@
                                    <th>@Sales.NetAmount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="goods-receipt-cancelled" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Sales.TransNo</th>
                                    <th class="width-80">@Sales.TransDate</th>
                                    @*<th class="width-80">@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th class="width-120">@Sales.CustomerLocation</th>*@
                                    <th class="width-80">@Sales.NetAmount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Sales.InvoiceNo</th>
                                    <th>@Sales.InvoiceDate</th>
                                    @*<th>@Sales.SalesType</th>*@
                                    <th>@Sales.CustomerName</th>
                                    @*<th>@Sales.CustomerLocation</th>*@
                                    <th>@Sales.NetAmount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/GoodsReceipt")
<script type="text/javascript">
    GoodsReceipt.list();
</script>