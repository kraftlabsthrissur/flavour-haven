@{
    ViewBag.Title = "Sales Order";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Sales.SalesOrder</h1>
            <span class="uk-text-upper uk-text-small">@Sales.SalesOrderList</span>
        </div>
        <div class="heading_actions">
            <div class="width-200">
                <div class="md-input-wrapper md-input-filled">
                    <div class="uk-form-file v1">
                        <label id="selected-file">
                            Upload XLS File
                        </label>
                        <label class="upload-btn md-btn md-btn-flat md-btn-wave waves-effect waves-button text-capitalize">
                            <i class="uk-icon-upload"></i>
                        </label>
                        <input id="select-file" type="file" tabindex="-1">
                    </div>
                </div>
            </div>
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "SalesOrder", new { Area = "Sales" }, new { @class = "md-btn" }))
            <input type="hidden" id="normalclass" value="@ViewBag.normalclass" />
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
