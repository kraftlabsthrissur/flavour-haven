<div class="uk-modal " id="uploded-orders" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Uploded Orders<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="Uploded Orders"></i></h3>
        </div>
        <table id="uploded-orders-list" class="uk-table" cellspacing="0">
            <thead>
                <tr>
                    <th class="width-10">@Common.SerialNo</th>
                    <th class="width-20">@Sales.PackingCode</th>
                    <th class="width-100">@Sales.PackingName</th>
                    <th class="width-20">@Sales.ItemCode</th>
                    <th class="width-100">@Sales.ItemName</th>
                    <th class="width-120">@Sales.Qty</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div id="notice" class="uk-form-row"></div>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOkUplodedOrders">Process</button>
        </div>
    </div>
</div>

