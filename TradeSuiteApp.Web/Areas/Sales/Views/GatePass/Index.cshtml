@model IEnumerable<TradeSuiteApp.Web.Areas.Sales.Models.GatePassModel>
@{
    ViewBag.Title = Sales.GatePass;
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Sales.GatePass</h1>
            <span class="uk-text-upper uk-text-small">@Sales.GatePassList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "GatePass", new { Area = "Sales" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="gatePass_list">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-gate-pass',animation:'slide-horizontal'}" id="tabs-gatepass">
                    <li class="uk-active" data-tab="draft"><a>@Sales.Draft</a></li>
                    <li class="" data-tab="saved-gate-pass"><a>@Sales.SavedGatePass</a></li>
                    <li class="" data-tab="cancelled"><a>@Sales.Cancelled</a></li>
                </ul>
                <ul id="tabs-gate-pass" class="uk-switcher uk-margin">
                    <li>
                        <table class="uk-table" id="draft-list" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table class="uk-table" id="gate-pass-list" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table class="uk-table" id="cancelled-list" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>
                                        @Sales.TransNo
                                    </th>
                                    <th>
                                        @Sales.TransDate
                                    </th>
                                    <th>
                                        @Sales.VehicleNo
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/GatePass")
<script type="text/javascript">
    GatePass.list();
</script>