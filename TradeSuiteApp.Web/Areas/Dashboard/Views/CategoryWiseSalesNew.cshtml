@model TradeSuiteApp.Web.Models.DashBoardModel
<div class="md-card" data-uk-grid-margin="">
    <div class="md-card-content uk-row-first">
        <div class="heading_a uk-margin-bottom">Category Wise Sales </div>
        <div class="Category-Wise">
            <div class="uk-width-1-1">
                <ul class="uk-tab" data-uk-tab="{connect:'#category-wise-sales'}" id="category-wise-sales-tab">
                    <li class="uk-active" data-tab="month-wise" id="category-wise-sales-month-tab"><a>@DateTime.Now.ToString("MMMM")</a></li>
                    <li data-tab="year-wise" id="category-wise-sales-year-tab"><a>@DateTime.Now.ToString("yyyy")</a></li>
                </ul>
                <ul id="category-wise-sales" class="uk-switcher uk-margin">
                    <li aria-hidden="false" class="uk-active">
                        <div>
                            <table id="category-wise-sales-month-table" class="uk-table">
                                <thead>
                                    <tr>
                                        <td>Sales Category</td>
                                        <td class="width-80">Budget In Kg</td>
                                        <td class="width-80">Actual In Kg</td>
                                        <td class="width-80">Budget In Lakhs</td>
                                        <td class="width-80">Actual In Lakhs</td>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </li>
                    <li aria-hidden="true" class="">
                        <div>
                            <table id="category-wise-sales-year-table" class="uk-table">
                                <thead>
                                    <tr>
                                        <td>Sales Category</td>
                                        <td class="width-80">Budget In Kg</td>
                                        <td class="width-80">Actual In Kg</td>
                                        <td class="width-80">Budget In Lakhs</td>
                                        <td class="width-80">Actual In Lakhs</td>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </li>

                </ul>
            </div>
        </div>
    </div>
</div>
