@model TradeSuiteApp.Web.Areas.Manufacturing.Models.ProductionPackingScheduleModel

@{
    ViewBag.Title = "Create Production Packing Schedule";
}


<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Manufacturing.PackingSchedule</h1>
            <span class="uk-text-upper uk-text-small">@Manufacturing.CreatePackingSchedule</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn save'>" + Common.Save + "</a>")
            @ActionAuthorization.IsAuthorized("SaveAsDraft", "<a class='md-btn save-as-draft'>" + Common.SaveAsDraft + "</a>")

            @Html.ActionLink(Common.Close, "Index", "ProductionPackingSchedule", new { Area = "Manufacturing" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view production">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Manufacturing/Views/ProductionPackingSchedule/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/ProductionPackingSchedule")
@Scripts.Render("~/Js/Page/Item")
<script type="text/javascript">
    ProductionPackingSchedule.init();
</script>