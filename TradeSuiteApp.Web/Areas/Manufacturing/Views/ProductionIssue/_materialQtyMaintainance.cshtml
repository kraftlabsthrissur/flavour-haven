<button data-uk-modal="{target:'#materialQtymaintainance',center:false }" id="btnMaterialQtyMaintaince" style="display:none;"></button>
<div class="uk-modal" id="materialQtymaintainance" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Add Material <i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <br />
        <div class="uk-form-row" id="add-item-wrapper">
            <div class="uk-grid" data-uk-grid-margin>
                <input type="hidden" id="hdnMaterialMaintainanceItemID" />
                <input type="hidden" id="ProductionIssueMaterialID" />
                <div class="uk-width-medium-4-10">
                    <label for="uk_dp_start">@Manufacturing.ItemName</label>
                    <input type="text" class="md-input label-fixed" value="" id="txtMaterialMaintanceItemName" readonly />
                </div>
                <div class="uk-width-medium-2-10">
                    <label for="uk_dp_start">@Manufacturing.UOM</label>
                    <input type="text" class="md-input label-fixed" id="txtMaterialMaintainceUOM" readonly />
                </div>

                <div class="uk-width-medium-2-10">
                    <label for="uk_dp_start">@Manufacturing.IssueQty</label>
                    <input type="text" class="md-input label-fixed mask-production-qty  txtMaterialManageIssueQty" id="txtMaterialManageIssueQty" />
                </div>
                <div class="uk-width-medium-2-10">
                    <div class="md-input-wrapper md-input-filled">
                        <label>&nbsp;</label>
                        <button class="md-btn md-btn-primary md-btn-block md-btn-wave-light waves-effect waves-button waves-light" id="btnAddMaterialQty">@Common.Add</button>
                    </div>
                </div>
            </div>
        </div>
        <table id="tblMaterialQtyMaintenance" class="uk-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th class="width-20">@Common.SerialNo</th>
                    <th>@Manufacturing.ItemName</th>
                    <th class="width-60">@Manufacturing.UOM</th>
                    <th class="width-60">@Manufacturing.IssueQty</th>
                    <th class="width-80">@Manufacturing.BatchNo</th>
                    <th class="width-80">@Manufacturing.IssueDate</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOkMaterialMaintenance">Ok</button>
        </div>
    </div>
</div>