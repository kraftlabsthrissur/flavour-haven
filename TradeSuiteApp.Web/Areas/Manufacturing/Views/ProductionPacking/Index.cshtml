@model IEnumerable<TradeSuiteApp.Web.Areas.Manufacturing.Models.PackingViewModel>
@{
    ViewBag.Title = "Production Packing";

}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Manufacturing.ProductionPacking</h1>
            <span class="uk-text-upper uk-text-small">@Manufacturing.ProductionPacking</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "ProductionPacking", new { Area = "Manufacturing" }, new { @class = "md-btn" }))
            
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab hai" data-uk-tab="{connect:'#tabs-modules-content',animation:'slide-horizontal'}" id="tabs-module">
                    <li class="uk-active"data-tab="draft"><a>@Manufacturing.Draft</a></li>
                    <li data-tab="ongoing"><a>@Manufacturing.OnGoing</a></li>
                    <li data-tab="packed"><a>@Manufacturing.Packed</a></li>
                    <li data-tab="cancelled"><a>@Manufacturing.Cancelled</a></li>
                </ul>
                <ul id="tabs-modules-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>

                    <li>
                        <table id="ongoing-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="packed-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
