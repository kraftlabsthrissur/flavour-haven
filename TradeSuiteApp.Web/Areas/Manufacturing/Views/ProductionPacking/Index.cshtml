                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="cancelled-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ProductionGroup</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.PackedQty</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/ProductionPacking")
<script type="text/javascript">
    ProductionPacking.list();
</script>