
@{
    ViewBag.Title = "PartialView";
}


<div class="uk-modal" id="select-issue" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-large" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">@Manufacturing.SelectJobWorkIssue<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="issue-list" class="uk-table" cellspacing="0">
            <thead>
                <tr>
                    <th class="width-10">@Common.SerialNo</th>
                    <th class="width-10"></th>
                    <th class="width-40">@Manufacturing.IssueNo</th>
                    <th class="width-200">@Manufacturing.SupplierName</th>
                    <th class="width-120">@Manufacturing.IssueDate</th>
                    
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th>@Common.SerialNo</th>
                    <th></th>
                    <th>@Manufacturing.IssueNo</th>
                    <th>@Manufacturing.SupplierName</th>
                    <th>@Manufacturing.IssueDate</th>
                    
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOKIssue">Ok</button>
        </div>
    </div>
</div>


