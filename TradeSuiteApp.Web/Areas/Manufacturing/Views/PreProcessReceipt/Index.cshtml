@model List<TradeSuiteApp.Web.Areas.Manufacturing.Models.PreProcessReceiptDisplayViewModel>
@{
    ViewBag.Title = "Material Purification Receipt List";

}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Manufacturing.MaterialPurification</h1>
            <span class="uk-text-upper uk-text-small">@Manufacturing.MaterialPurification</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "PreProcessReceipt", new { Area = "Manufacturing" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-preprocess-receipt-content',animation:'slide-horizontal'}" id="tabs-preprocess-receipt">
                    <li class="uk-active" data-tab="draft"><a>@Manufacturing.Draft</a></li>
                    <li class="" data-tab="saved-preprocess-receipt"><a>@Manufacturing.SavedPurificationReceipt</a></li>
                    <li class="" data-tab="cancelled"><a>@Manufacturing.Cancelled</a></li>
                </ul>
                <ul id="tabs-preprocess-receipt-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.ReceiptQty</th>
                                    <th class="width-80">@Manufacturing.Activity</th>
                                    <th class="width-80">@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.ReceiptQty</th>
                                    <th>@Manufacturing.Activity</th>
                                    <th>@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="saved-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.ReceiptQty</th>
                                    <th class="width-80">@Manufacturing.Activity</th>
                                    <th class="width-80">@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.ReceiptQty</th>
                                    <th>@Manufacturing.Activity</th>
                                    <th>@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="cancelled-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th class="width-40">@Manufacturing.Unit</th>
                                    <th class="width-80">@Manufacturing.ReceiptQty</th>
                                    <th class="width-80">@Manufacturing.Activity</th>
                                    <th class="width-80">@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>@Common.SerialNo</th>
                                    <th>@Manufacturing.ReceiptNo</th>
                                    <th>@Manufacturing.IssueItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.IssueQty</th>
                                    <th>@Manufacturing.ReceiptItem</th>
                                    <th>@Manufacturing.Unit</th>
                                    <th>@Manufacturing.ReceiptQty</th>
                                    <th>@Manufacturing.Activity</th>
                                    <th>@Manufacturing.QuantityLoss</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/PreProcessReceipt")
<script type="text/javascript">
    PreProcessReceipt.list();
</script>




