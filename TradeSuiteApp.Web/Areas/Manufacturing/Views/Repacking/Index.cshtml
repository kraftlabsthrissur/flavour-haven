@model IEnumerable<TradeSuiteApp.Web.Areas.Manufacturing.Models.RepackingViewModel>
@{
    ViewBag.Title = "Repacking";

}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Manufacturing.Repacking</h1>
            <span class="uk-text-upper uk-text-small">@Manufacturing.Repacking</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "Repacking", new { Area = "Manufacturing" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-repacking-content',animation:'slide-horizontal'}" id="tabs-repacking">
                    <li class="uk-active" data-tab="draft"><a>@Manufacturing.Draft</a></li>
                    <li class="" data-tab="saved-repacking"><a>@Manufacturing.SavedRepacking</a></li>
                    <li class="" data-tab="cancelled"><a>@Manufacturing.Cancelled</a></li>
                </ul>
                <ul id="tabs-repacking-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="saved-repacking-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="cancelled-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Manufacturing.TransNo</th>
                                    <th class="width-80">@Manufacturing.TransDate</th>
                                    <th>@Manufacturing.ItemName</th>
                                    <th class="width-80">@Manufacturing.BatchNo</th>
                                    <th class="width-80">@Manufacturing.BatchType</th>
                                    <th class="width-80">@Manufacturing.QuantityIn</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/Repacking")
<script type="text/javascript">
    Repacking.list();
</script>


