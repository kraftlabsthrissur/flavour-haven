@model TradeSuiteApp.Web.Areas.Manufacturing.Models.ProductionProcessViewModel

@{
    ViewBag.Title = "Create Production Process ";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_actions">
            <a data-uk-tooltip="{pos:'bottom'}" title="Print"><i class="md-icon material-icons">&#xE8AD;</i></a>
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>@Common.Action</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <h1>@Manufacturing.ProductionProcess</h1>
        <span class="uk-text-upper uk-text-small">@Manufacturing.CreateProductionProcess</span>
    </div>
    <div id="page_content_inner" class="form-view production">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="md-card-toolbar">
                    <button class="md-btn btnSave"> Save</button>
                    <button class="md-btn btnSaveASDraft"> Save As Draft</button>
                    @Html.HiddenFor(m => m.ID)
                    @Html.ActionLink(Common.Close, "Index", "ProductionProcess", new { Area = "Manufacturing" }, new { @class = "md-btn " })
                </div>
                <h3 class="heading_a"></h3>

                @{Html.RenderPartial("/Areas/Manufacturing/Views/ProductionProcess/Form.cshtml", Model); }



                <div class="md-card-toolbar md-card-toolbar-bottom">
                    <button class="md-btn btnSave"> Save</button>
                    <button class="md-btn btnSaveASDraft"> Save As Draft</button>
                    @Html.ActionLink(Common.Close, "Index", "ProductionProcess", new { Area = "Manufacturing" }, new { @class = "md-btn " })
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/ProductionProcess")






