@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.AdvanceRequestModel>
@{
    ViewBag.Title = "Advance Request ";

}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.AdvanceRequestTitle</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.AdvanceRequestTitle</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "AdvanceRequest", new { Area = "Accounts" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-advance-request-content',animation:'slide-horizontal'}" id="tabs-advance-request">
                    <li class="uk-active" data-tab="saved"><a>@Accounts.SavedAdvanceRequest</a></li>
                    <li class="" data-tab="approved"><a>@Accounts.Approved</a></li>
                    <li class="" data-tab="processed"><a>@Accounts.Processed</a></li>
                    <li class="" data-tab="suspended"><a>@Accounts.Suspended</a></li>
                </ul>
                <ul id="tabs-advance-request-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="saved-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-60">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>

                    <li>
                        <table id="approved-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-60">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>

                    <li>
                        <table id="processed-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-60">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>

                    <li>
                        <table id="suspended-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-60">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">@Common.SerialNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestNo</th>
                                    <th class="width-80">@Accounts.AdvanceRequestDate</th>
                                    <th>@Masters.EmployeeName</th>
                                    <th class="width-80">@Accounts.Amount</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>

</div>
@Scripts.Render("~/Js/Page/AdvanceRequest")

<script type="text/javascript">
    advance_request.list();
</script>
