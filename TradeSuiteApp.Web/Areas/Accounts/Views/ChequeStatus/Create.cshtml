@model TradeSuiteApp.Web.Areas.Accounts.Models.ChequeStatusModel
@{
    ViewBag.Title = Accounts.ChequeStatus;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.ChequeStatus</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.CreateChequeStatus</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveAsDraft'>" + Common.SaveAsDraft + "</a>")
           
            @Html.ActionLink(Common.Close, "Index", "ChequeStatus", new { Area = "Accounts" }, new { @class = "md-btn " })
            @Html.HiddenFor(m => m.UserStateID)
            @Html.HiddenFor(m => m.IGST)
            @Html.HiddenFor(m => m.CGST)
            @Html.HiddenFor(m => m.SGST)
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/Accounts/Views/ChequeStatus/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Html.DropDownList("StatusTemplateReceived", new SelectList(ViewBag.StatusReceived, "Value", "Text"), new { @class="uk-hidden"})
@Html.DropDownList("StatusTemplateDeposited", new SelectList(ViewBag.StatusDeposited, "Value", "Text"), new { @class = "uk-hidden" })
@Html.DropDownList("StatusTemplateCollected", new SelectList(ViewBag.StatusCollected, "Value", "Text"), new { @class = "uk-hidden" })
@Html.DropDownList("StatusTemplateBounced", new SelectList(ViewBag.StatusBounced, "Value", "Text"), new { @class = "uk-hidden" })
@Html.DropDownList("StatusTemplatePendingError", new SelectList(ViewBag.StatusPendingError, "Value", "Text"), new { @class = "uk-hidden" })
@Html.DropDownList("StatusTemplateCancelled", new SelectList(ViewBag.StatusCancelled, "Value", "Text"), new { @class = "uk-hidden" })
@Scripts.Render("~/Js/Page/Accounts/ChequeStatus")
<script type="text/javascript">
    ChequeStatus.init();
</script>


