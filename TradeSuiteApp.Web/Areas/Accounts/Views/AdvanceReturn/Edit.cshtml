

@model TradeSuiteApp.Web.Areas.Accounts.Models.AdvanceReturnModel
@{
    ViewBag.Title = "Edit Advance Return";

}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.AdvanceReturn</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.AdvanceReturn</span>
        </div>
        <div class="heading_actions">
            @Html.HiddenFor(Model => Model.ID)
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveAndPost'>" + Common.SaveAndPost + "</a>")
            @ActionAuthorization.IsAuthorized("SaveAsDraft", "<a class='md-btn btnSaveDraft'>" + Common.SaveAsDraft + "</a>")
            @Html.ActionLink(Common.Close, "Index", "AdvanceReturn", new { Area = "Accounts" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    @{Html.RenderPartial("/Areas/Accounts/Views/AdvanceReturn/Form.cshtml", Model); }
</div>

@Scripts.Render("~/Js/Page/Supplier")
@Scripts.Render("~/Js/Page/Employee")
@Scripts.Render("~/Js/Page/AdvanceReturn")
<script type="text/javascript">
    advance_return.init();

</script>






