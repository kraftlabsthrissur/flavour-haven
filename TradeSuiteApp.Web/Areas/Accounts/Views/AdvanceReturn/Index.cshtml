
@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.AdvanceReturnModel>
@{
    ViewBag.Title = "Advance Return";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.AdvanceReturn</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.AdvanceReturn</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "AdvanceReturn", new { Area = "Accounts" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>@Common.Action</a></li>

                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-advance-return-content',animation:'slide-horizontal'}" id="tabs-advance-return">
                    <li class="uk-active" data-tab="draft"><a>@Accounts.Draft</a></li>
                    <li class="" data-tab="saved-advance-return"><a>@Accounts.SavedAdvanceReturn</a></li>
                </ul>
                <ul id="tabs-advance-return-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnDate
                                    </th>
                                    <th>
                                        @Accounts.Name
                                    </th>
                                    <th class="width-80">
                                        @Accounts.Category
                                    </th>
                                    <th class="width-80">
                                        @Accounts.NetAmount
                                    </th>

                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnDate
                                    </th>
                                    <th>
                                        @Accounts.Name
                                    </th>
                                    <th class="width-80">
                                        @Accounts.Category
                                    </th>
                                    <th class="width-80">
                                        @Accounts.NetAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="advance-return-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnDate
                                    </th>
                                    <th>
                                        @Accounts.Name
                                    </th>
                                    <th class="width-80">
                                        @Accounts.Category
                                    </th>
                                    <th class="width-80">
                                        @Accounts.NetAmount
                                    </th>

                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.AdvanceReturnDate
                                    </th>
                                    <th>
                                        @Accounts.Name
                                    </th>
                                    <th class="width-80">
                                        @Accounts.Category
                                    </th>
                                    <th class="width-80">
                                        @Accounts.NetAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/AdvanceReturn")
<script type="text/javascript">
    advance_return.advance_return_list();

</script>



