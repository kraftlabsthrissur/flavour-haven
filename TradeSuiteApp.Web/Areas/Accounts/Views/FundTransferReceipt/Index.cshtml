@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.FundTransferReceiptModel>
@{
    ViewBag.Title = @Accounts.FundTransferReceiptList;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.FundTransferReceipt</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.FundTransferReceiptList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "FundTransferReceipt", new { Area = "Accounts" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <table id="Receipt-list" class="uk-table">
                    <thead>
                        <tr>
                        <tr>
                            <th class="width-20">
                                @Common.SerialNo
                            </th>
                            <th class="width-80">
                                @Accounts.TransNo
                            </th>
                            <th class="width-80">
                                @Accounts.TransDate
                            </th>
                            <th>
                                @Accounts.FromLocation
                            </th>
                            <th>
                                @Accounts.ToLocation
                            </th>
                            <th class="width-80">
                                @Accounts.ModeOfPayment
                            </th>
                            <th class="width-80">
                                @Accounts.TotalAmount
                            </th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th class="width-20">
                                @Common.SerialNo
                            </th>
                            <th class="width-80">
                                @Accounts.TransNo
                            </th>
                            <th class="width-80">
                                @Accounts.TransDate
                            </th>
                            <th>
                                @Accounts.FromLocation
                            </th>
                            <th>
                                @Accounts.ToLocation
                            </th>
                            <th class="width-80">
                                @Accounts.ModeOfPayment
                            </th>
                            <th class="width-80">
                                @Accounts.TotalAmount
                            </th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/FundTransferReceipt")

<script type="text/javascript">
    FundTransferReceipt.list();
</script>

