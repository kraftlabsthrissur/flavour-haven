@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.FundTransferModel>
@{
    ViewBag.Title = @Accounts.ContraVoucherList;
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.ContraVoucher</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.ContraVoucherList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "FundTransfer", new { Area = "Accounts" }, new { @class = "md-btn" }))
            
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab hai" data-uk-tab="{connect:'#tabs-saved-fund-transfer',animation:'slide-horizontal'}" id="tabs-savedFundTranasfer">
                    <li class="uk-active" data-tab="draft"><a>@Accounts.Draft</a></li>
                    <li class="" data-tab="saved-fund-transfer"><a>@Accounts.SavedContraVoucher</a></li>
                </ul>
                <ul id="tabs-saved-fund-transfer" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.FundTransferNo
                                    </th>
                                    <th class="width-80">
                                        @Html.DisplayNameFor(model => model.Date)
                                    </th>
                                    <th>
                                        @Accounts.FromLocation
                                    </th>
                                    <th>
                                        @Accounts.ToLocation
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.FundTransferNo
                                    </th>
                                    <th class="width-80">
                                        @Html.DisplayNameFor(model => model.Date)
                                    </th>
                                    <th>
                                        @Accounts.FromLocation
                                    </th>
                                    <th>
                                        @Accounts.ToLocation
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </li>
                    <li>
                        <table id="fund-transfer-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.FundTransferNo
                                    </th>
                                    <th class="width-80">
                                        @Html.DisplayNameFor(model => model.Date)
                                    </th>
                                    <th>
                                        @Accounts.FromLocation
                                    </th>
                                    <th>
                                        @Accounts.ToLocation
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.FundTransferNo
                                    </th>
                                    <th class="width-80">
                                        @Html.DisplayNameFor(model => model.Date)
                                    </th>
                                    <th>
                                        @Accounts.FromLocation
                                    </th>
                                    <th>
                                        @Accounts.ToLocation
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </li>
                    </ul>

</div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/FundTransfer")




