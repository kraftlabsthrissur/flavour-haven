
@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.BRSModel>
@{
    ViewBag.Title = Accounts.BankReconciliationStatementList;
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.BankReconciliationStatement</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.BankReconciliationStatementList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "CreateV3", "BRS", new { Area = "Accounts" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-brs-content',animation:'slide-horizontal'}" id="tabs-brs">
                    <li class="uk-active" data-tab="to-be--reconcile"><a>@Accounts.PendingReconcilation</a></li>
                    <li class="" data-tab="reconciled"><a>@Accounts.Reconciled</a></li>
                </ul>
                <ul id="tabs-brs-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="to-be--reconcile-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.DocumentType
                                    </th>
                                    
                                    <th class="width-80">
                                        @Accounts.DocumentNumber
                                    </th>

                                    <th>
                                        @Accounts.AccountName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.TransactionDate
                                    </th>
                                    <th class="width-80">
                                        @Accounts.BankName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DebitAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.CreditAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ToBeReconcileDate
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.DocumentType
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DocumentNumber
                                    </th>

                                    <th>
                                        @Accounts.AccountName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.TransactionDate
                                    </th>
                                    <th class="width-80">
                                        @Accounts.BankName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DebitAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.CreditAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ToBeReconcileDate
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="reconciled-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.DocumentType
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DocumentNumber
                                    </th>

                                    <th>
                                        @Accounts.AccountName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.TransactionDate
                                    </th>
                                    <th class="width-80">
                                        @Accounts.BankName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DebitAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.CreditAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ReconciledDate
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.DocumentType
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DocumentNumber
                                    </th>

                                    <th>
                                        @Accounts.AccountName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.TransactionDate
                                    </th>
                                    <th class="width-80">
                                        @Accounts.BankName
                                    </th>

                                    <th class="width-80">
                                        @Accounts.DebitAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.CreditAmount
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ReconciledDate
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/BRSV3")
<script type="text/javascript">
    BRS.list();
</script>





