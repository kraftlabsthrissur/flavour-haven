@model IEnumerable<TradeSuiteApp.Web.Areas.Accounts.Models.BankExpensesModel>
@{
    ViewBag.Title = @Accounts.FinancialExpensesList;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.FinancialExpenses</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.FinancialExpensesList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "BankExpenses", new { Area = "Accounts" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab" data-uk-tab="{connect:'#tabs-financial-expenses-content',animation:'slide-horizontal'}" id="tabs-financial-expenses">
                    <li class="uk-active" data-tab="draft"><a>@Accounts.Draft</a></li>
                    <li class="" data-tab="saved-financial-expenses"><a>@Accounts.SavedFinancialExpenses</a></li>
                </ul>
                <ul id="tabs-financial-expenses-content" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransDate
                                    </th>
                                    <th>
                                        @Accounts.ItemName
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransDate
                                    </th>
                                    <th>
                                        @Accounts.ItemName
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="saved-bank-expences-list" class="uk-table">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransDate
                                    </th>
                                    <th>
                                        @Accounts.ItemName
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransNo
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TransDate
                                    </th>
                                    <th>
                                        @Accounts.ItemName
                                    </th>
                                    <th class="width-80">
                                        @Accounts.ModeOfPayment
                                    </th>
                                    <th class="width-80">
                                        @Accounts.TotalAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/BankExpenses")




