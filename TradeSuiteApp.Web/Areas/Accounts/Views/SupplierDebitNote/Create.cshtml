@model TradeSuiteApp.Web.Areas.Accounts.Models.SupplierDebitNoteModel

@{
    ViewBag.Title = Accounts.CreateSupplierDebitNote;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Accounts.SupplierDebitNote</h1>
            <span class="uk-text-upper uk-text-small">@Accounts.CreateSupplierDebitNote</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveDn'>" + Common.Save + "</a>")
            @ActionAuthorization.IsAuthorized("SaveAsDraft", "<a class='md-btn btnSaveASDraft'>" + Common.SaveAsDraft + "</a>")
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSaveDnNew'>" + Common.SaveAndNew + "</a>")            
            @Html.ActionLink(Common.Close, "Index", "SupplierDebitNote", new { Area = "Accounts" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="section-group">
                    @{Html.RenderPartial("/Areas/Accounts/Views/SupplierDebitNote/Form.cshtml", Model); }
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/Supplier")
@Scripts.Render("~/Js/Page/Item")
@Scripts.Render("~/Js/Page/SupplierDebitNote")
@Html.Partial("~/Areas/Masters/Views/Supplier/PartialList.cshtml")
@Scripts.Render("~/Js/Page/Item")
@Html.Partial("~/Areas/Masters/Views/Item/ItemList.cshtml")
<script type="text/javascript">
    SupplierDebitNote.init();
</script>

