<div class="uk-modal" id="add-request" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog uk-modal-dialog-medium" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title">Select Stock Request<i class="material-icons" data-uk-tooltip="{pos:'top'}" title="headline tooltip"></i></h3>
        </div>
        <table id="requisition-list" class="uk-table">
            <thead>
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-20"></th>
                    <th class="width-20">@Stock.RequestNo</th>
                    <th class="width-20">@Stock.RequestDate</th>
                    <th class="width-20">@Stock.IssueLocation</th>
                    <th class="width-20">@Stock.IssuePremise</th>
                    <th class="width-20">@Stock.ReceiptLocation</th>
                    <th class="width-20">@Stock.ReceiptPremise</th>
                    <th class="width-40">@Stock.ProductionSchedule_Item</th>
                    <th class="width-20">@Stock.BatchNo</th>
                </tr>
            </thead>
            <thead class="search">
                <tr>
                    <th class="width-30">@Common.SerialNo</th>
                    <th class="width-20"></th>
                    <th class="width-20">@Stock.RequestNo</th>
                    <th class="width-20">@Stock.RequestDate</th>
                    <th class="width-20">@Stock.IssueLocation</th>
                    <th class="width-20">@Stock.IssuePremise</th>
                    <th class="width-20">@Stock.ReceiptLocation</th>
                    <th class="width-20">@Stock.ReceiptPremise</th>
                    <th class="width-40">@Stock.ProductionSchedule_Item</th>
                    <th class="width-20">@Stock.BatchNo</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary uk-modal-close" id="btnOkStockRequisitionList">Ok</button>
        </div>
    </div>
</div>
