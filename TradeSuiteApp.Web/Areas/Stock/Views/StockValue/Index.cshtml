
@{
    ViewBag.Title = "Stock Movement (As on date)";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_title">
            <h1>@Stock.StockMovement as on @ViewBag.ProcessedDate</h1>
            <span class="uk-text-upper uk-text-small">@Stock.StockMovementList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Execute", " <a class='md-btn' id='process'>" + @Common.Process + "</a>")

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>@Common.Action</a></li>

                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view stock-value">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <table id="stock-value-list" class="uk-table">
                    <thead>
                        <tr>
                            <th class="width-40">
                                @Common.SerialNo
                            </th>
                            <th class="width-100">
                                @Stock.ItemName
                            </th>
                            <th class="width-80">
                                @Stock.OpeningStock
                            </th>
                            <th class="width-60">
                                @Stock.OpeningRate
                            </th>
                            <th class="width-80">
                                @Stock.OpeningValue
                            </th>
                            <th class="width-60">
                                @Stock.ReceivedStock
                            </th>
                            <th class="width-60">
                                @Stock.ReceivedRate
                            </th>
                            <th class="width-60">
                                @Stock.ReceivedValue
                            </th>
                            <th class="width-60">
                                @Stock.IssueStock
                            </th>
                            <th class="width-60">
                                @Stock.IssueRate
                            </th>
                            <th class="width-60">
                                @Stock.IssueValue
                            </th>
                            <th class="width-80">
                                @Stock.ClosingStock
                            </th>
                            <th class="width-60">
                                @Stock.ClosingRate
                            </th>
                            <th class="width-80">
                                @Stock.ClosingValue
                            </th>
                            <th class="width-60">
                                @Stock.LastUpdatedRate
                            </th>
                            <th class="width-100">
                                @Stock.LastUpdatedDate
                            </th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th>
                                @Common.SerialNo
                            </th>
                            <th>
                                @Stock.ItemName
                            </th>
                            <th>
                                @Stock.OpeningStock
                            </th>
                            <th>
                                @Stock.OpeningRate
                            </th>
                            <th>
                                @Stock.OpeningValue
                            </th>
                            <th>
                                @Stock.ReceivedStock
                            </th>
                            <th>
                                @Stock.ReceivedRate
                            </th>
                            <th>
                                @Stock.ReceivedValue
                            </th>
                            <th>
                                @Stock.IssueStock
                            </th>
                            <th>
                                @Stock.IssueRate
                            </th>
                            <th>
                                @Stock.IssueValue
                            </th>
                            <th>
                                @Stock.ClosingStock
                            </th>
                            <th>
                                @Stock.ClosingRate
                            </th>
                            <th>
                                @Stock.ClosingValue
                            </th>
                            <th>
                                @Stock.LastUpdatedRate
                            </th>
                            <th>
                                @Stock.LastUpdatedDate
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                       
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/StockValue")
<script type="text/javascript">
    stockvalue.list();
</script>