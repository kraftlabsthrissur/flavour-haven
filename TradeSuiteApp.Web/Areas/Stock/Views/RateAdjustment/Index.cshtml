@model IEnumerable<TradeSuiteApp.Web.Areas.Stock.Models.RateAdjustmentModel>
@{
    ViewBag.Title = "Index";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Stock.RateAdjustment</h1>
            <span class="uk-text-upper uk-text-small">@Stock.RateAdjustmentList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Create", Html.ActionLink(Common.Create, "Create", "RateAdjustment", new { Area = "Stock" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab hai" data-uk-tab="{connect:'#tabs-rate-adjustment',animation:'slide-horizontal'}" id="tabs-RateAdjustment">
                    <li class="uk-active" data-tab="draft"><a>@Stock.Draft</a></li>
                    <li class="" data-tab="rate-adjustment"><a>@Stock.SavedRateAdjustment</a></li>
                </ul>
                <ul id="tabs-rate-adjustment" class="uk-switcher uk-margin">
                    <li>
                        <table id="draft-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @Stock.TransNo
                                    </th>
                                    <th>
                                        @Stock.TransDate
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @Stock.TransNo
                                    </th>
                                    <th>
                                        @Stock.TransDate
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="rate-adjustment-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-20">
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @Stock.TransNo
                                    </th>
                                    <th>
                                        @Stock.TransDate
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @Stock.TransNo
                                    </th>
                                    <th>
                                        @Stock.TransDate
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/RateAdjustment")
<script type="text/javascript">
    rateadjustment.list();
</script>




