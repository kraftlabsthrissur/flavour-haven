@model IEnumerable<TradeSuiteApp.Web.Areas.Stock.Models.StockAdjustmentModel>
@{
    ViewBag.Title = "Index";
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Stock.StockAdjustment</h1>
            <span class="uk-text-upper uk-text-small">@Stock.StockAdjustmentList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("CreateV3", Html.ActionLink(Common.Create, "CreateV3", "StockAdjustment", new { Area = "Stock" }, new { @class = "md-btn" }))

            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">

                <table id="stock-adjustment-list" class="uk-table" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>
                                @Common.SerialNo
                            </th>
                            <th>
                                @Stock.TransNo
                            </th>
                            <th>
                                @Stock.TransDate
                            </th>

                            <th>
                                @Stock.ItemName
                            </th>


                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th>
                                @Common.SerialNo
                            </th>
                            <th>
                                @Stock.TransNo
                            </th>
                            <th>
                                @Stock.TransDate
                            </th>

                            <th>
                                @Stock.ItemName
                            </th>



                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/StockAdjustmentV3")
<script type="text/javascript">
    stockadjustment.list();
</script>



