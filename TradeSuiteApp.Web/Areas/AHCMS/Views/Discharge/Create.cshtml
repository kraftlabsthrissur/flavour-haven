
@model TradeSuiteApp.Web.Areas.AHCMS.Models.DischargeModel
@{
    ViewBag.Title = AHCMS.DischargeSummary;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@AHCMS.DischargeSummary</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.DischargeSummaryDatails</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Print", "<a class='md-btn btnPrint'>" + Common.Print + "</a>")
            @if (!Model.IsDischarged)
            {
            @ActionAuthorization.IsAuthorized("Discharge", "<a class='md-btn btnDischarged'>" + AHCMS.Discharge + "</a>")
            }
            @Html.ActionLink(Common.Close, "Index", "Discharge", new { Area = "AHCMS" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/AHCMS/Views/Discharge/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/Discharge")
<script type="text/javascript">
    Discharge.init();
</script>







