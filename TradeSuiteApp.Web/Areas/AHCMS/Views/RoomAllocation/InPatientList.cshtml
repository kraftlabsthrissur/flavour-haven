
@model IEnumerable<TradeSuiteApp.Web.Areas.AHCMS.Models.RoomAllocationModel>
@{
    ViewBag.Title = @AHCMS.InPatientList;
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>InPatient</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.InPatientList</span>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <table id="inpatient-list" class="uk-table">
                    <thead>
                        <tr>
                            <th class="width-30">
                                @Common.SerialNo
                            </th>
                            <th>
                                @AHCMS.Patient
                            </th>
                            <th>
                                @AHCMS.Room
                            </th>
                            <th class="uk-width-medium-1-8">
                                @AHCMS.AdmissionDate
                            </th>
                            <th class="width-40"></th>
                            <th class="width-40"></th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th>
                                @Common.SerialNo
                            </th>
                            <th>
                                @AHCMS.Patient
                            </th>
                            <th>
                                @AHCMS.Room
                            </th>
                            <th>
                                @AHCMS.AdmissionDate
                            </th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/RoomAllocation")
<script type="text/javascript">
    RoomAllocation.init();
    RoomAllocation.inpatient_list();
</script>


