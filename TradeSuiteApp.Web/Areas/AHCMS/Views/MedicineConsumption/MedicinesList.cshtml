
<input type="button" class="uk-hidden" id="show-medicine-list" data-uk-modal="{target:'#medicine-list', center:false}" />
<div class="uk-modal" id="medicine-list" aria-hidden="true" style="display: none; overflow-y: scroll;">
    <div class="uk-modal-dialog" style="">
        <div class="uk-modal-header">
            <h3 class="uk-modal-title"> Medicine List<i class="material-icons"></i></h3>
        </div>
        <div class="uk-modal-content">
            <div class="uk-form-row">
                <table id="medicine-list" class="uk-table">
                    <thead>
                        <tr>
                            <th>@Common.SerialNo</th>
                            <th></th>
                            <th>@AHCMS.Medicine</th>
                            <th>@Sales.Stock</th>
                            <th>@Sales.Qty</th>
                            <th>@Sales.Unit</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
        <div class="uk-modal-footer uk-text-right">
            <button type="button" class="md-btn uk-modal-close">Close</button>
            <button type="button" class="md-btn md-btn-primary" id="btnOkMedicines">Ok</button>
        </div>
    </div>
</div>
