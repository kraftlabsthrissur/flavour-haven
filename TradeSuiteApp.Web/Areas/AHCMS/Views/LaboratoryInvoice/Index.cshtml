

@{
    ViewBag.Title = AHCMS.LabTest;
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@AHCMS.LabTest</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.LabTestList</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("CreateInvoice", Html.ActionLink(Common.Create, "CreateInvoice", "LaboratoryInvoice", new { Area = "AHCMS" }, new { @class = "md-btn" }))
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab labouratoryinvoice" data-uk-tab="{connect:'#tabs-labouratory-invoice',animation:'slide-horizontal'}" id="tabs-labouratoryinvoice">
                    <li class="uk-active" data-tab="ToBeInvoiced"><a>@AHCMS.ToBeInvoiced</a></li>
                    <li class="" data-tab="Invoiced"><a>@AHCMS.Invoiced</a></li>
                </ul>
                <ul id="tabs-labouratory-invoice" class="uk-switcher uk-margin">
                    <li>
                        <table id="to-be-invoiced-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th width="30">
                                        @Common.SerialNo
                                    </th>
                                    <th width="30">
                                        @AHCMS.TransNo
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.Date
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.MRDNo
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.LabTest
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.Doctor
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @AHCMS.TransNo
                                    </th>
                                    <th>
                                        @AHCMS.Date
                                    </th>
                                    <th>
                                        @AHCMS.MRDNo
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.LabTest
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="invoiced-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th width="30">
                                        @Common.SerialNo
                                    </th>
                                    <th width="30">
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.Date
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.MRDNo
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.LabTest
                                    </th>
                                    <th class="width-100">
                                        @AHCMS.Doctor
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th>
                                        @Common.SerialNo
                                    </th>
                                    <th>
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th>
                                        @AHCMS.Date
                                    </th>
                                    <th>
                                        @AHCMS.MRDNo
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.LabTest
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/LaboratoryInvoice")

<script type="text/javascript">
    LaboratoryInvoice.list();
</script>



