@model TradeSuiteApp.Web.Areas.AHCMS.Models.LabTestModel
<div class="uk-modal-header">
    <h3 class="uk-modal-title">Consultation Fee<i class="material-icons"></i></h3>
</div>
<div class="uk-modal-content">
    <div class="uk-form-row">
        <div class="uk-grid text-center uk-margin">
            <div class="uk-width-medium-1-8">
                <label>@AHCMS.Date</label>
                @Html.TextBoxFor(model => model.Date, new { @class = "md-input label-fixed", @readonly = "readonly", @disabled = "disabled" })
            </div>
            <div class="uk-width-medium-2-10">
                <label>@AHCMS.LabTestType</label>
                @Html.DropDownList("Type", new List<SelectListItem>
                        {
                            new SelectListItem{ Text="Internal", Value = "Internal" },
                            new SelectListItem{ Text="External", Value = "External"} }, new { @class = "md-input label-fixed" })
            </div>
            <div class="uk-width-medium-2-5">
                <label>@AHCMS.Supplier</label>
                @Html.DropDownListFor(model => model.SupplierID, Model.SupplierList, new { @class = "md-input label-fixed" })
            </div>
        </div>
    </div>
</div>
<div class="uk-modal-footer uk-text-right">
    <button type="button" class="md-btn  uk-modal-close">@Common.Close</button>
    <button type="button" class="md-btn md-btn-primary" id="btnSaveAppointment">Save</button>
</div>
