@model TradeSuiteApp.Web.Areas.AHCMS.Models.AppointmentScheduleModel
@{
    ViewBag.Title = AHCMS.CreateNewAppointment;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@AHCMS.AppointmentScheduling</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.CreateNewAppointment</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")
            @Html.HiddenFor(m => m.EmployeeCategoryID)
            @Html.HiddenFor(model => model.IsAllowConsultationSchedule)
            @Html.ActionLink(Common.Close, "Index", "AppointmentSchedule", new { Area = "AHCMS" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        @{Html.RenderPartial("/Areas/AHCMS/Views/AppointmentSchedule/Form.cshtml", Model); }
    </div>
</div>

@Html.Partial("~/Areas/Masters/Views/Employee/DoctorList.cshtml")
@Html.Partial("~/Areas/AHCMS/Views/AppointmentSchedule/PatientList.cshtml", Model)
@Html.Partial("~/Areas/AHCMS/Views/AppointmentSchedule/AddPatient.cshtml", Model)
@Html.Partial("~/Areas/AHCMS/Views/AppointmentSchedule/AddDiscount.cshtml", Model)
@Html.Partial("~/Areas/AHCMS/Views/AppointmentSchedule/Token.cshtml")

@Scripts.Render("~/Js/Page/Employee")
@Scripts.Render("~/Js/Page/OutPatient")
@Scripts.Render("~/Js/Page/AppointmentSchedule")
@Scripts.Render("~/Js/Page/InternationalPatient")
<script type="text/javascript">
    AppointmentSchedule.init();
</script>

