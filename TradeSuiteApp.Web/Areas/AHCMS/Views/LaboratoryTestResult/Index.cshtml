@model IEnumerable<TradeSuiteApp.Web.Areas.AHCMS.Models.LaboratoryTestResultModel>
@{
    ViewBag.Title = "Laboratory Test Result";
}

<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_title">
            <h1>@AHCMS.LaboratoryTestResult</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.LaboratoryTestResultList</span>
        </div>
        <div class="heading_actions">         
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>@Common.Action</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <ul class="uk-tab hai" data-uk-tab="{connect:'#tabs-lab-test',animation:'slide-horizontal'}" id="tabs-labtest">
                    <li class="uk-active" data-tab="invoiced"><a>@AHCMS.InvoicedLabTest</a></li>
                    <li class="" data-tab="completed"><a>@AHCMS.CompletedLabTest</a></li>
                </ul>
                <ul id="tabs-lab-test" class="uk-switcher uk-margin">
                    <li>
                        <table id="invoiced-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-10">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceDate
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                    <th>
                                        @AHCMS.NetAmount
                                    </th>

                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-10">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceDate
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                    <th>
                                        @AHCMS.NetAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                    <li>
                        <table id="completed-list" class="uk-table" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="width-10">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceDate
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                    <th>
                                        @AHCMS.NetAmount
                                    </th>
                                </tr>
                            </thead>
                            <thead class="search">
                                <tr>
                                    <th class="width-10">
                                        @Common.SerialNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceNo
                                    </th>
                                    <th class="width-80">
                                        @AHCMS.InvoiceDate
                                    </th>
                                    <th>
                                        @AHCMS.Patient
                                    </th>
                                    <th>
                                        @AHCMS.Doctor
                                    </th>
                                    <th>
                                        @AHCMS.NetAmount
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Js/Page/LaboratoryTestResult")
<script type="text/javascript">
    LaboratoryTestResult.list();
</script>

