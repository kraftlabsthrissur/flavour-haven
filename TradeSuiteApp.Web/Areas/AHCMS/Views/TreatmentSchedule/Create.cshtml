@model TradeSuiteApp.Web.Areas.AHCMS.Models.TreatmentScheduleModel
@{
    ViewBag.Title = @AHCMS.CreateTreatmentSchedule;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>Treatment Scheduling</h1>
            <span class="uk-text-upper uk-text-small">Create Treatment</span>
        </div>
        <div class="heading_actions">
            @ActionAuthorization.IsAuthorized("Save", "<a class='md-btn btnSave'>" + Common.Save + "</a>")

            @Html.ActionLink(Common.Close, "Index", "TreatmentSchedule", new { Area = "AHCMS" }, new { @class = "md-btn " })
            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>Print</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/AHCMS/Views/TreatmentSchedule/Form.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Html.Partial("~/Areas/Sales/Views/_Version3/CounterSales/AppointmentProcessList.cshtml")
@Scripts.Render("~/Js/Page/CounterSales")
@Scripts.Render("~/Js/Page/TreatmentSchedule")
<script type="text/javascript">
    TreatmentSchedule.init();
    </script>



