@model TradeSuiteApp.Web.Areas.AHCMS.Models.TreatmentScheduleModel
@{
    ViewBag.Title = @AHCMS.TreatmentScheduleDetails;
}
<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <div class="heading_title">
            <h1>@AHCMS.TreatmentSchedule</h1>
            <span class="uk-text-upper uk-text-small">@AHCMS.TreatmentScheduleDetails</span>
        </div>
        <div class="heading_actions">
            @Html.ActionLink(Common.Back, "Index", "TreatmentSchedule", new { Area = "AHCMS" }, new { @class = "md-btn " })


            <div data-uk-dropdown>
                <i class="md-icon material-icons">&#xE5D4;</i>
                <div class="uk-dropdown uk-dropdown-small">
                    <ul class="uk-nav">
                        <li><a>@Common.Action</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div id="page_content_inner" class="form-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                @{Html.RenderPartial("/Areas/AHCMS/Views/TreatmentSchedule/view.cshtml", Model); }
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/PrescriptionFormat")
<script type="text/javascript">
    PrescriptionFormat.init();
</script>



