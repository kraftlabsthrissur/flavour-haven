
@{
    ViewBag.Title = "PrintList";
}



<div id="page_content">
    <div id="page_heading" data-uk-sticky="{ top: 54, media: 960 }">
        <div class="heading_title">
            <h1>@Masters.PrintList</h1>
            <span class="uk-text-upper uk-text-small">@Masters.PrintList</span>
        </div>
    </div>
    <div id="page_content_inner" class="list-view">
        <div class="md-card uk-margin-medium-bottom">
            <div class="md-card-content">
                <div class="uk-width-medium-1-10">
                    <div class="uk-input-group">
                        <label>@Masters.FromDate</label>
                        @Html.TextBox("DateFrom", (string)ViewBag.Date, new { @class = "md-input label-fixed  past-date date" })
                        <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar past-date"></i></span>
                    </div>
                </div>
                <table id="Print-list" class="uk-table">
                    <thead>
                        <tr>
                            <th width="30">@Common.SerialNo</th>
                            <th>@Masters.TransNo</th>
                            <th>@Masters.Form</th>
                            <th>@Masters.Date</th>
                            <th>@Masters.Print</th>
                            <th>@Masters.GenerateFile</th>
                        </tr>
                    </thead>
                    <thead class="search">
                        <tr>
                            <th>@Common.SerialNo</th>
                            <th>@Masters.TransNo</th>
                            <th>@Masters.Form</th>
                            <th>@Masters.Date</th>
                            <th>@Masters.Print</th>
                            <th>@Masters.GenerateFile</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Js/Page/PrintList")
<script type="text/javascript">
    PrintList.init();
</script>


